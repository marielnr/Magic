	//function - get the Params Name of start page
	//return - a json obj of the Params Name of start page
	function getStartPageToolParamsName() {
		//console.log("inside getStartPageToolParamsName ,returning spbStartPageApi_startPageParams = "+ spbStartPageApi_startPageParams);
		return spbStartPageApi_startPageParams;
	}
	
	//function - get the Tool Params of start page
	//return - a stringify json obj of the Tool Params of start page
	function getStartPageToolParamsVal() {
		var lStartPageToolParamsJsonObj ={};
		
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.StartPageURLParam] = spbStartPageApi.getStartPageControlURL();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.CloudDomainURLParam] = spbStartPageApi.getStartPageCloudDomainURL();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.ProdcutParam] = spbStartPageApi.getStartPageToolProduct();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.InstallModeParam] = spbStartPageApi.getStartPageToolInstallMode();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.AppModeParam] = spbStartPageApi.getStartPageToolAppMode();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.LicenseParam] = spbStartPageApi.getStartPageToolLicense();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.InvocationModeParam] = spbStartPageApi.getStartPageToolInvocationMode();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.AppNameParam] = spbStartPageApi.getStartPageToolAppName();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.IsAutomationParam] = spbStartPageApi.getStartPageToolIsAnalyticsEnabled();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.VersionParam] = spbStartPageApi.getStartPageToolVersion();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.PatchParam] = spbStartPageApi.getStartPageToolPatch();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.IsrParam] = spbStartPageApi.getStartPageToolIsr();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.QirParam] = spbStartPageApi.getStartPageToolQir();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.DemoParam] = spbStartPageApi.getStartPageToolIsDemo();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.TrialParam] = spbStartPageApi.getStartPageToolIsTrial();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.InstalledVersionParam] = spbStartPageApi.getStartPageToolInstalledVersion();
		lStartPageToolParamsJsonObj[spbStartPageApi_startPageParams.SuccessParam] = true;
		//console.log("inside getStartPageToolParamsVal ,returning stringigy lStartPageToolParamsJsonObj = "+ JSON.stringify(lStartPageToolParamsJsonObj));
		return JSON.stringify(lStartPageToolParamsJsonObj);
	}

	//function - set the Tool Params of start page
	//@pStartPageToolParamsJsonObj - a json obj of the Tool Params of start page
	function setStartPageToolParamsVal(pStartPageToolParamsJsonObj) {
		//console.log("inside setStartPageToolParamsVal ,pStartPageToolParamsJsonObj  = "+ pStartPageToolParamsJsonObj);
		if(pStartPageToolParamsJsonObj) {
			var lStartPageUrlParam =spbStartPageApi_startPageParams.StartPageURLParam;
			var lCloudDomainURLParam =spbStartPageApi_startPageParams.CloudDomainURLParam;
			var lProdcutParam =spbStartPageApi_startPageParams.ProdcutParam;
			var lInstallModeParam =spbStartPageApi_startPageParams.InstallModeParam;
			var lAppModeParam =spbStartPageApi_startPageParams.AppModeParam;
			var lLicenseParam =spbStartPageApi_startPageParams.LicenseParam;
			var lInvocationModeParam =spbStartPageApi_startPageParams.InvocationModeParam;
			var lAppNameParam =spbStartPageApi_startPageParams.AppNameParam;
			var lIsAutomationParam =spbStartPageApi_startPageParams.IsAutomationParam;
			var lVersionParam =spbStartPageApi_startPageParams.VersionParam;
			var lPatchParam =spbStartPageApi_startPageParams.PatchParam;
			var lIsrParam =spbStartPageApi_startPageParams.IsrParam;
			var lQirParam =spbStartPageApi_startPageParams.QirParam;
			var lDemoParam =spbStartPageApi_startPageParams.DemoParam;
			var lTrialParam =spbStartPageApi_startPageParams.TrialParam;
			var lInstalledVersionParam =spbStartPageApi_startPageParams.InstalledVersionParam;
			var lSuccessParam =spbStartPageApi_startPageParams.SuccessParam;
			
			if(pStartPageToolParamsJsonObj[lCloudDomainURLParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lCloudDomainURLParam] ="+pStartPageToolParamsJsonObj[lCloudDomainURLParam]);
				spbStartPageApi.setStartPageCloudDomainURL(pStartPageToolParamsJsonObj[lCloudDomainURLParam]);
				
				var lStartPageControlURL = "";
				if(pStartPageToolParamsJsonObj[lStartPageUrlParam]) {
					//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lStartPageUrlParam] ="+pStartPageToolParamsJsonObj[lStartPageUrlParam]);
					lStartPageControlURL = pStartPageToolParamsJsonObj[lStartPageUrlParam];
				} else {
					lStartPageControlURL = pStartPageToolParamsJsonObj[lCloudDomainURLParam] + "/startpage";
				}
				spbStartPageApi.setStartPageControlURL(lStartPageControlURL);
			}
			
			if(pStartPageToolParamsJsonObj[lProdcutParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lProdcutParam] ="+pStartPageToolParamsJsonObj[lProdcutParam]);
				spbStartPageApi.setStartPageToolProduct(pStartPageToolParamsJsonObj[lProdcutParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lInstallModeParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lInstallModeParam] ="+pStartPageToolParamsJsonObj[lInstallModeParam]);
				spbStartPageApi.setStartPageToolInstallMode(pStartPageToolParamsJsonObj[lInstallModeParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lAppModeParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lAppModeParam] ="+pStartPageToolParamsJsonObj[lAppModeParam]);
				spbStartPageApi.setStartPageToolAppMode(pStartPageToolParamsJsonObj[lAppModeParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lLicenseParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lLicenseParam] ="+pStartPageToolParamsJsonObj[lLicenseParam]);
				spbStartPageApi.setStartPageToolLicense(pStartPageToolParamsJsonObj[lLicenseParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lInvocationModeParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lInvocationModeParam] ="+pStartPageToolParamsJsonObj[lInvocationModeParam]);
				spbStartPageApi.setStartPageToolInvocationMode(pStartPageToolParamsJsonObj[lInvocationModeParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lAppNameParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lAppNameParam] ="+pStartPageToolParamsJsonObj[lAppNameParam]);
				spbStartPageApi.setStartPageToolAppName(pStartPageToolParamsJsonObj[lAppNameParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lIsAutomationParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lIsAutomationParam] ="+pStartPageToolParamsJsonObj[lIsAutomationParam]);
				spbStartPageApi.setStartPageToolIsAnalyticsEnabled(pStartPageToolParamsJsonObj[lIsAutomationParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lVersionParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lVersionParam] ="+pStartPageToolParamsJsonObj[lVersionParam]);
				spbStartPageApi.setStartPageToolVersion(pStartPageToolParamsJsonObj[lVersionParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lPatchParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lPatchParam] ="+pStartPageToolParamsJsonObj[lPatchParam]);
				spbStartPageApi.setStartPageToolPatch(pStartPageToolParamsJsonObj[lPatchParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lIsrParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lIsrParam] ="+pStartPageToolParamsJsonObj[lIsrParam]);
				spbStartPageApi.setStartPageToolIsr(pStartPageToolParamsJsonObj[lIsrParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lQirParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lQirParam] ="+pStartPageToolParamsJsonObj[lQirParam]);
				spbStartPageApi.setStartPageToolQir(pStartPageToolParamsJsonObj[lQirParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lDemoParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lDemoParam] ="+pStartPageToolParamsJsonObj[lDemoParam]);
				spbStartPageApi.setStartPageToolIsDemo(pStartPageToolParamsJsonObj[lDemoParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lTrialParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lTrialParam] ="+pStartPageToolParamsJsonObj[lTrialParam]);
				spbStartPageApi.setStartPageToolIsTrial(pStartPageToolParamsJsonObj[lTrialParam]);	 
			}
			
			if(pStartPageToolParamsJsonObj[lInstalledVersionParam]) {
				//console.log("inside setStartPageToolParamsVal , pStartPageToolParamsJsonObj[lInstalledVersionParam] ="+pStartPageToolParamsJsonObj[lInstalledVersionParam]);
				spbStartPageApi.setStartPageToolInstalledVersion(pStartPageToolParamsJsonObj[lInstalledVersionParam]);	 
			}
		}
	}
	
	//function - get the Maximum Display Length of RSS Feed
	//return - max Rss Feed Display Length
	function getMaxRssFeedDisplayLength() {
		//console.log("inside getMaxRssFeedDisplayLength ,returning spbStartPageApi_maxRssFeedDisplayLength = "+ spbStartPageApi_maxRssFeedDisplayLength);
		return spbStartPageApi_maxRssFeedDisplayLength;
	}

	//function - set the Maximum Display Length of RSS Feed
	//@p_maxRssFeedDisplayLength - Maximum Display Length of RSS Feed
	function setMaxRssFeedDisplayLength(p_maxRssFeedDisplayLength) {
		//console.log("inside setMaxRssFeedDisplayLength ,setting spbStartPageApi_maxRssFeedDisplayLength = "+ p_maxRssFeedDisplayLength);
		spbStartPageApi_maxRssFeedDisplayLength = p_maxRssFeedDisplayLength;  
	}
	
	//function - get the URI Param Local Key of cloud start page
	//return - a string of the URI Param Local Key of cloud start page
	function getStartPageURIParamLocalKey() {
		//console.log("inside getStartPageURIParamLocalKey ,returning spbStartPageApi_startPageURIParamLocalKey = "+ spbStartPageApi_startPageURIParamLocalKey);
		return spbStartPageApi_startPageURIParamLocalKey;
	}
	
	//function - get the URI Param Server Key of cloud start page
	//return - a string of the URI Param Server Key of cloud start page
	function getStartPageURIParamServerKey() {
		//console.log("inside getStartPageURIParamServerKey ,returning spbStartPageApi_startPageURIParamServerKey = "+ spbStartPageApi_startPageURIParamServerKey);
		return spbStartPageApi_startPageURIParamServerKey;
	}
	
	//function - get IsURIParamLocalKey of cloud start page
	//return - In case,IsURIParamLocalKey of cloud start page is set, then return true else false
	function getStartPageIsURIParamLocalKey() {
		//console.log("inside getStartPageIsURIParamLocalKey ,returning spbStartPageApi_startPageIsURIParamLocalKey = "+ spbStartPageApi_startPageIsURIParamLocalKey);
		return spbStartPageApi_startPageIsURIParamLocalKey;
	}
	
	//function - set the IsURIParamLocalKey of cloud start page
	//p_startPageURIParamLocalKeyValue - a boolean type of the URI Param Local Key value of cloud start page
	function setStartPageIsURIParamLocalKey(p_startPageIsURIParamLocalKey) {
		//console.log("inside setStartPageIsURIParamLocalKey ,setting spbStartPageApi_startPageIsURIParamLocalKey = "+ p_startPageIsURIParamLocalKey);
		spbStartPageApi_startPageIsURIParamLocalKey = p_startPageIsURIParamLocalKey;
	}
	
	//function - get the value of given URI Param key from URL
	//@p_strURIParamKey - URI Param key for which value will be returned
	//return - return value of URI Param if URI Param key Exists else return "ERROR"
	function getURIParamValue(p_strURIParamKey) {
		var lURIParamStr = window.location.search.substring(1);
		//console.log("lURIParamStr = "+lURIParamStr);
		
		if(lURIParamStr && "" != lURIParamStr) {
			var lArrURIParams = lURIParamStr.split('&');
			var lURIParamsLength = lArrURIParams.length;
			var lURIParamCounter = 0;
			var lURIParamKeyNValue= "";

			for (lURIParamCounter = 0; lURIParamCounter < lURIParamsLength; lURIParamCounter++) {
				lURIParamKeyNValue = lArrURIParams[lURIParamCounter].split('=');

				if (lURIParamKeyNValue[0] === p_strURIParamKey) {
					return lURIParamKeyNValue[1] === undefined ? "ERROR" : decodeURIComponent(lURIParamKeyNValue[1]);
				}
			}
		} 
		
		return "ERROR";
	};
	
}(jQuery));
 
