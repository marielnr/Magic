/**
*File Name :- OrCADStartPageClientApi.js
*/

//Note startPageClientApi is generic namespace for clientApi Interfaces and For Each product client Api, this namespace will remain same except the functionality and File Name will change

// create namespace object if it doesn't exist
var startPageClientApi = startPageClientApi || {};

// closure keeps everything within out of global namespace
(function ($) { "use strict";
	var startPageClientApi_userEventsData = "";
		
	startPageClientApi.newDocument = newDocument;
	startPageClientApi.openDocument = openDocument;
	startPageClientApi.openDesign = openDesign;
	startPageClientApi.openURLinBrowser = openURLinBrowser;
	startPageClientApi.startTCL = startTCL;
	startPageClientApi.getUserEventsData = getUserEventsData;
	startPageClientApi.insertUserEventsData = insertUserEventsData;
	startPageClientApi.logRevulyticsUserEventsData = logRevulyticsUserEventsData;
	startPageClientApi.getFavoriteAndRecent = getFavoriteAndRecent;
	
	//function - create new document
	function newDocument(p_strKey,pCallback) {
		if(p_strKey) {
			startPageClientApi.insertUserEventsData(p_strKey);
		} else {
			startPageClientApi.insertUserEventsData("newDocument");
		}
		
		if(undefined != pCallback && null != pCallback) {
			startPageClientApi.startTCL("::orcad::startpage::jsbridge::newCapDocument", pCallback);
		} else {
			startPageClientApi.startTCL("::orcad::startpage::jsbridge::newCapDocument", null);
		}
	}    

	//function - open a dialog to open an existing Document
	function openDocument(p_strKey,pCallback) {
		if(p_strKey) {
			startPageClientApi.insertUserEventsData(p_strKey);
		} else {
			startPageClientApi.insertUserEventsData("openDocument");
		}
		
		if(undefined != pCallback && null != pCallback) {
			startPageClientApi.startTCL("::orcad::startpage::jsbridge::openCapDocument", pCallback);
		} else {
			startPageClientApi.startTCL("::orcad::startpage::jsbridge::openCapDocument", null);
		}
	}
	
	//function - open an design with given design path
	function openDesign(p_strDesignName,pCallback) {
		startPageClientApi.insertUserEventsData("link_openDesign");
		
		var obj = {};
        obj.dsnname = p_strDesignName;
		
		if(undefined != pCallback && null != pCallback) {
			startPageClientApi.startTCL("::orcad::startpage::jsbridge::openDesign {" + JSON.stringify(obj) + "}", pCallback);
		} else {
			startPageClientApi.startTCL("::orcad::startpage::jsbridge::openDesign {" + JSON.stringify(obj) + "}", null);
		}
	}
	
	//function - opens a URL in a browser window
	//@inURL - the URL to attempt to open
	function openURLinBrowser(inURL,p_strID,pCallback) {
		var lbStartPageisURIParamLocalKey = spbStartPageApi.getStartPageIsURIParamLocalKey();
		
		if(lbStartPageisURIParamLocalKey) {
			window.open(inURL);
		}
		else {
			if(p_strID) {
				startPageClientApi.insertUserEventsData(p_strID);
			} else {
				startPageClientApi.insertUserEventsData("openURLinBrowser");
			}
			
			//console.log("enter openURLinBrowser with url = "+inURL);
			var obj = {};
			obj.url = inURL;
			
			if(undefined != pCallback && null != pCallback) {
				startPageClientApi.startTCL("::orcad::startpage::jsbridge::launchBrowser {" + JSON.stringify(obj) + "}", pCallback);
			} else {
				startPageClientApi.startTCL("::orcad::startpage::jsbridge::launchBrowser {" + JSON.stringify(obj) + "}", null);
			}
		}
	}
	
	//function - To get Favorite and Recent Projects Data in Json and return the same
	function getFavoriteAndRecent(pCallback) {
		if(undefined != pCallback && null != pCallback) {
			startPageClientApi.startTCL("::orcad::startpage::jsbridge::getFavoriteAndRecent", pCallback);
		} else {
			startPageClientApi.startTCL("::orcad::startpage::jsbridge::getFavoriteAndRecent", null);
		}
	}
	
	//function - attempt run a tcl command directly, with parametesr
	//@method - the command to attempt
	//@params - any parameters necessary to run the command, written as a JSON object
	function startTCL(p_commandWithParams, pCallback) {
		var lCommand = p_commandWithParams;
		if(undefined != pCallback && null != pCallback) {
			var l_objCallbackClosure = OrDzCallBackManager.createClosure(this, pCallback,[]);
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, l_objCallbackClosure);
		}
		else {
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, null);
		}
	}
	
	//function - to get user events data
	function getUserEventsData() {
		return startPageClientApi_userEventsData;
	}
	
	//function - add new ebtry of user event data if not exists else update count of existing user event data
	//@p_strUserEventKeyId - the unique id of event data to add or update count entry
	function insertUserEventsData(p_strUserEventKeyId) {
		if (startPageClientApi_userEventsData == null || startPageClientApi_userEventsData == undefined || startPageClientApi_userEventsData == "") {
			startPageClientApi_userEventsData ={};
		}
		
		if(startPageClientApi_userEventsData[p_strUserEventKeyId]) {
			startPageClientApi_userEventsData[p_strUserEventKeyId]["count"] = startPageClientApi_userEventsData[p_strUserEventKeyId]["count"]+1;
		} else {
			startPageClientApi_userEventsData[p_strUserEventKeyId] = {};
			startPageClientApi_userEventsData[p_strUserEventKeyId]["count"] = 1;
		}
	}
	
	//function - to insert user event data in orPrmWebCompIE widget structure
	//@p_strUserEventsData - user Events Data, if not passed then check for existence of Default User Events Data in case not empty
	function logRevulyticsUserEventsData(p_strUserEventsData,pCallback) {
		var l_strUserEventsData = "";
		if(p_strUserEventsData && p_strUserEventsData != "") {
			l_strUserEventsData = p_strUserEventsData;
		}
		else {//Default User Event Data
			var l_objUserEventsDataJson = startPageClientApi.getUserEventsData();
			//console.log("startPageClientApi_userEventsData =");
			//console.log(l_objUserEventsDataJson);
			if(l_objUserEventsDataJson && l_objUserEventsDataJson !="" && Object.keys(l_objUserEventsDataJson).length != 0) {
				var l_strUserEventsData = JSON.stringify(l_objUserEventsDataJson);				
			}
		}
		
		if(l_strUserEventsData != "") {
			if(undefined != pCallback && null != pCallback) {
				startPageClientApi.startTCL("::orcad::startpage::jsbridge::logRevulyticsUserEventsData {" + l_strUserEventsData + "}",pCallback);
			} else {
				startPageClientApi.startTCL("::orcad::startpage::jsbridge::logRevulyticsUserEventsData {" + l_strUserEventsData + "}",null);
			}
		}
	}
	
}(jQuery));
 
