 * @preserve  2019 by CADENCE Design Systems, Inc. All rights reserved
 */

/**
*File Name :- OrCADStartPageRecentFiles.js
*/

// create namespace object if it doesn't exist
var orcadstartpagerecentfiles = orcadstartpagerecentfiles || {};

// closure keeps everything within out of global namespace
(function ($) { "use strict";
	orcadstartpagerecentfiles.init = init;
	orcadstartpagerecentfiles.getRecentFiles = getRecentFiles;
	orcadstartpagerecentfiles.refreshRecentDocumentList = refreshRecentDocumentList;
	//Note the orcadstartpagerecentfiles_sOpenDesignCalled variable is set when open design called from Recent files of startPage and unset after getting response of the open design command to avoid calling any other tcl command e.g,::orcad::startpage::jsbridge::getFavoriteAndRecent,till the response of open design come.By Default value of orcadstartpagerecentfiles_sOpenDesignCalled variable is zero
	var orcadstartpagerecentfiles_sOpenDesignCalled = 0; 

	//function - attempt to load the recent files from the spinfo.ini file
    function getRecentFiles() {
		if ( 0 == orcadstartpagerecentfiles_sOpenDesignCalled ) {
			var result;
			startPageClientApi.getFavoriteAndRecent(function(returnData) {
				try {
					result = JSON.parse(returnData);				
					if (result.success) {
						showRecentFiles(result);
					} else {
						showNoFiles("There are no Recent Documents to display.");
					}
				}
				catch(err) {
					getRecentFilesFailure(err);
				}
			});
		}
    }

    //function - show a message indicating that the files were unable to be loaded
    //@inError - the error object returned when trying to load the files
    function getRecentFilesFailure(inError) {
		showNoFiles("Cannot list all Recent Documents.");
    }

	//function - set up the recent files region
    function init() {
		getRecentFiles();
    }

    //function - set up click functionality for file items in the list
    function initRecentFilesClickFunctions() {
		$("#recentProjectsCarasol .card").on("click", function(){ openDesign(this.id); });
	}


    function openDesign(inID) {
		var inIDWithHash="#"+inID;
		var designName=$(inIDWithHash).attr("DesigName");
		var result;
		
		orcadstartpagerecentfiles_sOpenDesignCalled = 1;

		spbStartPageApi.openDesign(designName, function(returnData) {
			orcadstartpagerecentfiles_sOpenDesignCalled =0;
			try {
				result = JSON.parse(returnData);
				//if there was a message returned
				if (result.message) {
					//if the message is that the file was not found, report it to the user
					if (result.message.indexOf("not found")) {
						var l_eFuncRetStatus = { val: OrDzGlobalEnum.StatusCode.OK };
						//var l_strProductType = OrDzSession.sGetSession(l_eFuncRetStatus).getConfigObj(l_eFuncRetStatus).getProductType(l_eFuncRetStatus);
						var l_strProductType = spbStartPageApi.getStartPageToolAppName();
						var l_fnClosure = OrDzCallBackManager.createClosure(orcadstartpagerecentfiles, orcadstartpagerecentfiles.refreshRecentDocumentList, []);
						//note eCAPTURE_W_IDS_SP_CANNOT_OPEN_RECENT_DOCUMENT mapped to 2447 in orSMICapture.h,but to call from tcl,we need to call with hardcorded value and 48 type means (MB_OK|MB_ICONWARNING)
						OrDzSMI.sShowSMIMessage(2450, "ORCAP", l_strProductType, "48", [designName], l_fnClosure); 
						
					}
					else {
						orcadstartpagerecentfiles.refreshRecentDocumentList();
					}
				}
				else {
					orcadstartpagerecentfiles.refreshRecentDocumentList();
				}
			}
			catch(err) {
				orcadstartpagerecentfiles.refreshRecentDocumentList();
	    	}
		});		
    }	
		
	//function - refrest the list
	function refreshRecentDocumentList() {
		//console.log("enter refreshRecentDocumentList");
		getRecentFiles();
	}


	//function - show a message in the files list if there are no files for some reason
	//@inMessage - the message to show to the user
	function showNoFiles(inMessage) {		
    	$("#recentProjectsCarasol").empty();
    	$("<div class='container' id='recentProjectsError'>" + inMessage + "</div>").appendTo("#recentProjectsCarasol");
		$("#btn_prev").hide();
		$("#btn_next").hide();
	}
	
	//function - display the JSON data in the recent files list
	//@inResult - the JSON data representing the favorite and recent files list		
	function showRecentFiles(inResult) {
		$("#recentProjectsCarasol").empty();
		var favorites = inResult.data.favorite;
		var recent = inResult.data.recent;
		//if there are no favorites or recent files at all, then show a message to the user
		if ((favorites.length == 0) && (recent.length == 0)) {
			showNoFiles("There are no Recent Documents to display.");
		} else {			
			$("#btn_prev").show();
			$("#btn_next").show();
			var fileTypes = ["favorite", "recent"];
			var recentProjectsArr = [inResult.data.favorite, inResult.data.recent];			
			
			
			var fileName="";
			var fileNameWithPath="";
			
			var carasol_item_data_length=4;
			var carasol_item_data_index=0;
			var carasol_item_count=0;	
			//var lDefaultSrcPath="./images/recenProImg.png";
			var lDefaultSrcPath="data:image/png;base64,";
			
			var srcPath = lDefaultSrcPath;
			var carasolitemdivid="";
			var carasolitemdividwithhash="";
			var carasolitemrowdivid="";
			var carasolitemrowdividwithhash="";
			var carasolitemrowcolmd3divid="";
			var carasolitemrowcolmd3dividwithhash="";
			var carasolitemrowcolmd3carddivid="";
			var carasolitemrowcolmd3carddividwithhash="";
			var carasolitemrowcolmd3cardbodydivid="";
			var carasolitemrowcolmd3cardbodydividwithhash="";
			var carasolitemrowcolmd3cardfooterdivid="";
			var carasolitemrowcolmd3cardfooterdividwithhash="";
			
			var carasolitemrowcolmd3cardbodyimgdivid="";
			var carasolitemrowcolmd3cardbodyimgdividwithhash="";
			
			var totalRecentItemsCnter=0;
			
			$.each(recentProjectsArr, function (filesIndex) {
				if (recentProjectsArr[filesIndex]) {
					if (recentProjectsArr[filesIndex].length > 0) {
			            $.each(recentProjectsArr[filesIndex], function (index) {			            	
							
							fileNameWithPath=recentProjectsArr[filesIndex][index].filename;
							
							if(undefined != recentProjectsArr[filesIndex][index].imageName && null != recentProjectsArr[filesIndex][index].imageName) {
								srcPath = recentProjectsArr[filesIndex][index].imageName;
							} else {
								srcpath = lDefaultSrcPath;
							}
							
							//console.log("image base64 src path is = " + srcPath);
							
							fileName=spbStartPageApi.parseFileName(fileNameWithPath);
							
							carasol_item_data_index = totalRecentItemsCnter % carasol_item_data_length;
				
							carasol_item_count = Math.floor(totalRecentItemsCnter / carasol_item_data_length);
							
							
							carasolitemdivid = "recentProjectsCarasol-" + carasol_item_count;
							carasolitemdividwithhash ="#"+carasolitemdivid;																
							carasolitemrowdivid = carasolitemdivid + "-row";
							carasolitemrowdividwithhash = "#"+carasolitemrowdivid;
							carasolitemrowcolmd3divid = carasolitemrowdivid + "-colmd3-" + carasol_item_data_index;
							carasolitemrowcolmd3dividwithhash = "#"+carasolitemrowcolmd3divid;
							carasolitemrowcolmd3carddivid = carasolitemrowcolmd3divid + "-card-" + carasol_item_data_index;
							carasolitemrowcolmd3carddividwithhash = "#"+carasolitemrowcolmd3carddivid;
							carasolitemrowcolmd3cardbodydivid = carasolitemrowcolmd3carddivid + "-cardBody-" + carasol_item_data_index;
							carasolitemrowcolmd3cardbodydividwithhash = "#"+carasolitemrowcolmd3cardbodydivid;
							carasolitemrowcolmd3cardfooterdivid = carasolitemrowcolmd3carddivid + "-cardFooter-" + carasol_item_data_index;
							carasolitemrowcolmd3cardfooterdividwithhash = "#"+carasolitemrowcolmd3cardfooterdivid;
							carasolitemrowcolmd3cardbodyimgdivid = carasolitemrowcolmd3cardbodydivid + "-img-" + carasol_item_data_index;
							carasolitemrowcolmd3cardbodyimgdividwithhash = "#"+carasolitemrowcolmd3cardbodyimgdivid;
							
							
							if(carasol_item_data_index == 0) {									
					
								if(carasol_item_count == 0) {
									$("<div class='carousel-item active' id='" + carasolitemdivid + "'></div>").appendTo("#recentProjectsCarasol");				
								}
								else {
									$("<div class='carousel-item' id='" + carasolitemdivid + "'></div>").appendTo("#recentProjectsCarasol");
								}	
								
								$("<div class='row' id='" + carasolitemrowdivid + "'></div>").appendTo(carasolitemdividwithhash);
							}							
							
							$("<div class='col-md-3 col-sm-3' id='" + carasolitemrowcolmd3divid + "'></div>").appendTo(carasolitemrowdividwithhash);
							$("<div class='card' id='" + carasolitemrowcolmd3carddivid + "' DesigName='" +fileNameWithPath +"'></div>").appendTo(carasolitemrowcolmd3dividwithhash);
							$("<div class='card-body' id='" + carasolitemrowcolmd3cardbodydivid + "' DesigName='" +fileNameWithPath +"'></div>").appendTo(carasolitemrowcolmd3carddividwithhash);
							$("<div class='card-footer' id='" + carasolitemrowcolmd3cardfooterdivid + "' DesigName='" +fileNameWithPath +"'>" + fileName + "</div>").appendTo(carasolitemrowcolmd3carddividwithhash);					
																									
							$("<img class='img-fluid' id='" + carasolitemrowcolmd3cardbodyimgdivid + "' src='"+srcPath+"'></img>").appendTo(carasolitemrowcolmd3cardbodydividwithhash);
							totalRecentItemsCnter++;												
							
						});
					}
				}
			});
			//setup the click functionality now that the elements exist
			initRecentFilesClickFunctions();			
		}
    } 	
		
}(jQuery));
 
