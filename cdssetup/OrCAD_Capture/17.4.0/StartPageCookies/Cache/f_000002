// Marketo  Marketing Automation
(function () {
	//console.log("enter Marketo  Marketing Automation function");
	var didInit = false;
	function initMunchkin() {
		//console.log("enter Marketo  Marketing Automation initMunchkin function");
		if(didInit === false) {
			//console.log("enter Marketo  Marketing Automation initMunchkin function for setting didInit true");
			didInit = true;
			Munchkin.init('070-BII-206');
		}
	}
	
	var s = document.createElement('script');
	s.type = 'text/javascript';
	s.async = true;
	s.src = 'https://munchkin.marketo.net/munchkin.js';
	s.onreadystatechange = function() {
		//console.log("munchkin onreadystatechange function");
		if (this.readyState == 'complete' || this.readyState == 'loaded') {
			//console.log("munchkin readyState ="+this.readyState);
			initMunchkin();
		}
	};
	s.onload = initMunchkin;
	document.getElementsByTagName('head')[0].appendChild(s);		
})();

//HotJar  Heatmaps and Recordings
function loadHotJar(p_startPageCloudDomainUrl) {
	//console.log("enter loadHotJar withn p_startPageCloudDomainUrl="+p_startPageCloudDomainUrl);
	(function (p_cloudDomainUrl,h,o,t,j,a,r) {
		//console.log("enter HotJar  Heatmaps and Recordings function with p_cloudDomainUrl ="+p_cloudDomainUrl);
		h.hj=h.hj||function(){(h.hj.q=h.hj.q||[]).push(arguments)};
		if(p_cloudDomainUrl && p_cloudDomainUrl.indexOf("testpcb") != -1) {
			//console.log("setting hotjar settings for test cloud domain");
			h._hjSettings={hjid:1676838,hjsv:6};
		} else if(p_cloudDomainUrl && p_cloudDomainUrl.indexOf("stagepcb") != -1) {
			//console.log("setting hotjar settings for stage cloud domain");
			h._hjSettings={hjid:1676840,hjsv:6};
		}
		else {
			//console.log("setting hotjar settings for release cloud domain");
			h._hjSettings={hjid:1405100,hjsv:6};
		}
		a=o.getElementsByTagName('head')[0];
		r=o.createElement('script');r.async=1;		
		r.src=t+h._hjSettings.hjid+j+h._hjSettings.hjsv;
		a.appendChild(r);	
	})(p_startPageCloudDomainUrl,window,document,'https://static.hotjar.com/c/hotjar-','.js?sv=');
};

