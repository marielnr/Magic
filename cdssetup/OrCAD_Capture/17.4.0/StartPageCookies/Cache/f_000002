/**
*File Name :- SPBStartPageAnnouncements.js
*/

// create namespace object if it doesn't exist
var spbStartPageAnnouncements = spbStartPageAnnouncements || {};

// closure keeps everything within out of global namespace
(function ($) { "use strict";
	spbStartPageAnnouncements.init = init;
	spbStartPageAnnouncements.getRssFeeds = getRssFeeds;

	//function - attempt to load the rss feeds
    function getRssFeeds() {
	  	var promise = new Promise(function(resolve, reject) {
	    		try {
					var lInstallMode = spbStartPageApi.getStartPageToolInstallMode();
					if(null != lInstallMode && undefined != lInstallMode && "TIPSpice_Install" == lInstallMode) {
						var RssFeedurl = 'https://resources.orcad.com/schematic-capture-circuit-simulation.rss';
					} else{
						var RssFeedurl = 'https://resources.pcb.cadence.com/blog.rss';
					}
		
					feednami.load(RssFeedurl)
						.then(feed => {
							showRssFeeds(feed);		  
					})
				}
				catch(err) {
					//console.log("spbStartPageAnnouncements.getRssFeeds catch error ="+err);
					getRssFeedsFailure();
				}
		});

		return promise;	
    }

    //function - show a message indicating that the rss feeds were unable to be loaded
    //@inError - the error object returned when trying to load the rss feeds
    function getRssFeedsFailure(inError) {
		showNoRssFeeds("");
    }

	//function - set up the rss feeds region
    function init() {
		getRssFeeds();
    }

    //function - set up click functionality for rss items in the list
    function initRssFeedsClickFunctions() {
		//console.log("initializing click functionality on rss links");
		$("#announcement .card").on("click", function(){ openRssFeedInBrowser(this.id); });
	}

   
    function openRssFeedInBrowser(inID) {
		//console.log("enter openRssFeedInBrowser for Rss with ID = "+inID);
		var inIDWithHash="#"+inID;
		var lRssFeedUrl=$(inIDWithHash).attr("linkInfo");
		//console.log("Rss Feed Url is ="+lRssFeedUrl);
		
		if(lRssFeedUrl) {
			spbStartPageApi.openURLinBrowser(lRssFeedUrl,"link_openRssFeed");
		}
    }	

	//function - hide all rss feeds if loading of rss feeds fails due to some reasons
	//@inMessage - the message to show to the user
	function showNoRssFeeds(inMessage) {		
    	$("#announcement").empty();
	}
	
	//function - display the Rss Feed Response in the announcemnts
	//@inResult - the XML JSON data representing the RSS Feeds Data		
	function showRssFeeds(responseText) {
		$("#announcement").empty();
		if (responseText != null && responseText !=undefined && responseText["entries"] != null && responseText["entries"] != undefined) {
			//console.log("Rss Feeds Succeeds with result ="+responseText);
			
			var lResponseTextEntriesArrLength = responseText["entries"].length;
			
			if ( lResponseTextEntriesArrLength > 0 ) {
				var required_Length_of_RssFeed_Items = spbStartPageApi.getMaxRssFeedDisplayLength();
				var counter_of_RssFeed_Items = 0;
				var rssFeedItemTitle = "";
				var rssFeedItemLink = "";
				var rssFeedItemMediaContentUrl = "";
				
				var announcementItemCardDivId = "";
				var announcementItemCardDivIdWithHash = "";
				var announcementItemCardBodyDivId = "";
				var announcementItemCardBodyDivIdWithHash = "";
				var announcementItemCardBodyImgTiltDivId = "";
				var announcementItemCardBodyImgTiltDivIdWithHash = "";
				var announcementItemCardBodyImgTiltImgTagDivId = "";
				var announcementItemCardBodyImgTiltImgTagDivIdWithHash = "";
				var announcementItemCardBodyTextValDivId = "";
				var announcementItemCardBodyTextValDivIdWithHash = "";
				var announcementItemCardBodyTextValWithTextLeftPDivId = "";
				var announcementItemCardBodyTextValWithTextLeftPDivIdWithHash = "";
				var announcementItemCardBodyTextValWithTextRightPDivId = "";
				var announcementItemCardBodyTextValWithTextRightPDivIdWithHash = "";
				var announcementItemCardBodyTextValWithTextRightPSpanDivId = "";
				var announcementItemCardBodyTextValWithTextRightPSpanDivIdWithHash = "";
				var announcementStaticTextWithValReadMore = "Read More";
				
				for (counter_of_RssFeed_Items = 0; counter_of_RssFeed_Items < lResponseTextEntriesArrLength; counter_of_RssFeed_Items++) 
				{ // or "item" or whatever suits your feed
					var el = responseText["entries"][counter_of_RssFeed_Items]
					//console.log("el is="+el);
					
					if(counter_of_RssFeed_Items < required_Length_of_RssFeed_Items ) {					
						rssFeedItemTitle = el["title"];
						rssFeedItemLink = el["link"];
						rssFeedItemMediaContentUrl = el["media:content"]["@"]["url"];

						announcementItemCardDivId = "announcement-" + counter_of_RssFeed_Items;					
						announcementItemCardDivIdWithHash = "#"+announcementItemCardDivId;
						announcementItemCardBodyDivId = announcementItemCardDivId + "-CardBody";					
						announcementItemCardBodyDivIdWithHash = "#"+announcementItemCardBodyDivId;
						announcementItemCardBodyImgTiltDivId = announcementItemCardBodyDivId + "-ImgTilt";
						announcementItemCardBodyImgTiltDivIdWithHash = "#"+announcementItemCardBodyImgTiltDivId;
						announcementItemCardBodyImgTiltImgTagDivId = announcementItemCardBodyImgTiltDivId + "-ImgTag";
						announcementItemCardBodyImgTiltImgTagDivIdWithHash = "#"+announcementItemCardBodyImgTiltImgTagDivId;
						announcementItemCardBodyTextValDivId = announcementItemCardBodyDivId + "-TextVal";
						announcementItemCardBodyTextValDivIdWithHash = "#"+announcementItemCardBodyTextValDivId;
						announcementItemCardBodyTextValWithTextLeftPDivId = announcementItemCardBodyTextValDivId + "-TextLeftP";
						announcementItemCardBodyTextValWithTextLeftPDivIdWithHash = "#"+announcementItemCardBodyTextValWithTextLeftPDivId;
						announcementItemCardBodyTextValWithTextRightPDivId = announcementItemCardBodyTextValDivId + "-TextRightP";
						announcementItemCardBodyTextValWithTextRightPDivIdWithHash = "#"+announcementItemCardBodyTextValWithTextRightPDivId;
						announcementItemCardBodyTextValWithTextRightPSpanDivId = announcementItemCardBodyTextValWithTextRightPDivId + "-Span";
						announcementItemCardBodyTextValWithTextRightPSpanDivIdWithHash = "#"+announcementItemCardBodyTextValWithTextRightPSpanDivId;
						
						$("<div class='card' id='" + announcementItemCardDivId + "' linkInfo='" + rssFeedItemLink + "' title='" + rssFeedItemTitle + "'></div>").appendTo("#announcement");
						$("<div class='card-body' id='" + announcementItemCardBodyDivId + "'></div>").appendTo(announcementItemCardDivIdWithHash);
						$("<div class='imgTilt' id='" + announcementItemCardBodyImgTiltDivId + "'></div>").appendTo(announcementItemCardBodyDivIdWithHash);
						$("<img class='img-fluid' id='" + announcementItemCardBodyImgTiltImgTagDivId + "' src='"+rssFeedItemMediaContentUrl+"'></img>").appendTo(announcementItemCardBodyImgTiltDivIdWithHash);
						$("<div class='textValue' id='" + announcementItemCardBodyTextValDivId + "'></div>").appendTo(announcementItemCardBodyDivIdWithHash);
						$("<p class='text-left' id='" + announcementItemCardBodyTextValWithTextLeftPDivId + "'>" + rssFeedItemTitle + "</p>").appendTo(announcementItemCardBodyTextValDivIdWithHash);
						$("<p class='text-right' id='" + announcementItemCardBodyTextValWithTextRightPDivId + "'></p>").appendTo(announcementItemCardBodyTextValDivIdWithHash);
						$("<span class='rssLink' id='" + announcementItemCardBodyTextValWithTextRightPSpanDivId + "'>" + announcementStaticTextWithValReadMore +"</span>").appendTo(announcementItemCardBodyTextValWithTextRightPDivIdWithHash);
						
					} else {
						break;
					}
				}
				//setup the click functionality now that the elements exist
				initRssFeedsClickFunctions();
			}
		} else {
				//console.log("Rss Feeds fails with result ="+responseText);
				showNoRssFeeds();
		}
    } 			
}(jQuery));
 
/**
 * @preserve  2019 by CADENCE Design Systems, Inc. All rights reserved
 */

/**
*File Name :- SPBStartPageChannelPartnerInfos.js
*/

// create namespace object if it doesn't exist
var spbStartPageChannelPartnerInfos = spbStartPageChannelPartnerInfos || {};

// closure keeps everything within out of global namespace
(function ($) { "use strict";
	spbStartPageChannelPartnerInfos.init = init;
	spbStartPageChannelPartnerInfos.getChannelPartnerDetails = getChannelPartnerDetails;

	//function - attempt to load the channel partner details
    function getChannelPartnerDetails() {
	  	var promise = new Promise(function(resolve, reject) {
	    		try {					
					var lStartPageToolParamsNameJsonObj = spbStartPageApi.getStartPageToolParamsName();
					var lClientParam = lStartPageToolParamsNameJsonObj.ClientParam;

					var ccpInfoUrl="";
					var clientParamVal = spbStartPageApi.getClientApiKey();											
					
					ccpInfoUrl = spbStartPageApi.getStartPageControlURL() + "/ccpInfo?"+ lClientParam + "=" + encodeURIComponent(clientParamVal);
					//console.log("ccpInfoUrl ="+ccpInfoUrl);
					OrDzHttpInterface.sGetHttpInterface().sendRequest(ccpInfoUrl, null, showChannelPartnerDetails, null);
		    	}
		    	catch(err) {
					//console.log("spbStartPageChannelPartnerInfos.getChannelPartnerDetails catch error ="+err);
					getChannelPartnerDetailsFailure();
		    	}
		});

		return promise;	
    }

    //function - don't do anything
    //@inError - the error object returned when trying to load the channel partner details
    function getChannelPartnerDetailsFailure(inError) {
		//console.log("inside getChannelPartnerDetailsFailure inError="+inError);
    }

	//function - set up the channel partner region
    function init() {
		initChannelPartnerDetailsClickFunctions();
		getChannelPartnerDetails();
    }

    //function - set up click functionality for channel partner items
    function initChannelPartnerDetailsClickFunctions() {
		//console.log("initializing click functionality on channle partner details");
		$("#companyUrl").on("click", function(){ openChannelPartnerWebsiteInBrowser(this.id); });
	}

   
    function openChannelPartnerWebsiteInBrowser(inID) {
		//console.log("enter openChannelPartnerWebsiteInBrowser for channel partner website with ID = "+inID);
		var inIDWithHash="#"+inID;
		var l_strCompanyURLLinkInfo=$(inIDWithHash).attr("linkInfo");
		//console.log("channle partner website url l_strCompanyURLLinkInfo = "+l_strCompanyURLLinkInfo);
		
		if(l_strCompanyURLLinkInfo) {
			spbStartPageApi.openURLinBrowser(l_strCompanyURLLinkInfo,"link_browseCCPCompanyUrl");
		}			
    }	
	
	//function - display the channel partner details Response in the Local Support
	//@inResult - the JSON data representing the Channel partner details	
	function showChannelPartnerDetails(responseText) {
		if (responseText && ((typeof responseText) == "string")) {
			//console.log("channel partner details  Succeeds with result ="+responseText);
			
			var lStartPageToolParamsNameJsonObj = spbStartPageApi.getStartPageToolParamsName();	
			var lCompanyNameParam = lStartPageToolParamsNameJsonObj.CompanyNameParam;			
			var lPhoneParam = lStartPageToolParamsNameJsonObj.PhoneParam;
			var lEmailIdParam = lStartPageToolParamsNameJsonObj.EmailIdParam; 		
			var lWebsiteParam = lStartPageToolParamsNameJsonObj.WebsiteParam;
			
			var lObjResponseTextJson = JSON.parse(responseText);
			
			var lStrCompanyName = lObjResponseTextJson[lCompanyNameParam];
			var lStrEmailId = lObjResponseTextJson[lEmailIdParam];
			var lStrPhone = lObjResponseTextJson[lPhoneParam];			
			var lStrWebsiteLinkInfoURL = lObjResponseTextJson[lWebsiteParam];
			var lStrWebsiteDisplayURL = lStrWebsiteLinkInfoURL;
			
			if(lStrWebsiteLinkInfoURL && lStrWebsiteLinkInfoURL !="") {
				if(-1 != lStrWebsiteLinkInfoURL.indexOf("http://")) {
					var lArrWebsiteLinkInfoURLElements = lStrWebsiteLinkInfoURL.split("http://");
					if(2 == lArrWebsiteLinkInfoURLElements.length) {
						lStrWebsiteDisplayURL = lArrWebsiteLinkInfoURLElements[1];
						if(lStrWebsiteDisplayURL.indexOf("/") == lStrWebsiteDisplayURL.length -1) {
							lStrWebsiteDisplayURL = lStrWebsiteDisplayURL.substring(0,lStrWebsiteDisplayURL.length -1);
						}
					}
				}
				else if(-1 != lStrWebsiteLinkInfoURL.indexOf("https://")) {
					var lArrWebsiteLinkInfoURLElements = lStrWebsiteLinkInfoURL.split("https://");
					if(2 == lArrWebsiteLinkInfoURLElements.length) {
						lStrWebsiteDisplayURL = lArrWebsiteLinkInfoURLElements[1];
						if(lStrWebsiteDisplayURL.indexOf("/") == lStrWebsiteDisplayURL.length -1) {
							lStrWebsiteDisplayURL = lStrWebsiteDisplayURL.substring(0,lStrWebsiteDisplayURL.length -1);
						}
					}
				}
			}
			
			$("#companyName").text(lStrCompanyName);
			$("#companyEmail").text(lStrEmailId);
			$("#companyPhone").text(lStrPhone);			
			$("#companyUrl").text(lStrWebsiteDisplayURL);					
			$("#companyUrl").attr({linkInfo:lStrWebsiteLinkInfoURL});
		} 
    } 			
}(jQuery));
 
/**
* @preserve  2019 by CADENCE Design Systems, Inc. All rights reserved
