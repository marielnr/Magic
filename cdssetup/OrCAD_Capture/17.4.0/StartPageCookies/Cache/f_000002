/**
*File Name :- OrCADCaptureStartPage.js
*/

// create namespace object if it doesn't exist
var orcadstartpage = orcadstartpage || {};

// closure keeps everything within out of global namespace
(function ($) { "use strict";

	orcadstartpage.init = init;

	//when the document loads, run the initial function
	$(document).ready(function() {	
		var l_nCBStage = 0;
		init(l_nCBStage);
	});

	$(window).on("beforeunload", function() { 
		startPageClientApi.logRevulyticsUserEventsData();		
	});	

	//function - run some preparations
	function init(p_nCBStage,p_responseJsonObj) { 
		switch(p_nCBStage)
		{
			case 0:
				orSetAppTheme();
				//preload images
				spbStartPageApi.preloadImages([
					"./images/backGroundOrcadCapture.jpg",
				]);
				
				var l_fnClosure = OrDzCallBackManager.createClosure(null, orcadstartpage.init, [p_nCBStage+1]);
				startPageClientApi.startTCL("::orcad::startpage::jsbridge::getStartPageToolParams",l_fnClosure);
				break;
			
			case 1:
				var result;
				try {
					result = JSON.parse(p_responseJsonObj);

					if (result.success) {
						//console.log("::orcad::startpage::jsbridge::getStartPageToolParams return result ="+result);
						spbStartPageApi.setStartPageToolParamsVal(result);
					}
				}
				catch(err) {
					//console.log("::orcad::startpage::jsbridge::getStartPageToolParams return error ="+err);
					var lStartPageURIParamLocalKey = spbStartPageApi.getStartPageURIParamLocalKey();
					var lStartPageURIParamLocalKeyValue = spbStartPageApi.getURIParamValue(lStartPageURIParamLocalKey);
					
					if(lStartPageURIParamLocalKeyValue && typeof(lStartPageURIParamLocalKeyValue) == "string" && "TRUE" == lStartPageURIParamLocalKeyValue.toUpperCase()) {
						spbStartPageApi.setStartPageIsURIParamLocalKey(true);
						var lStartPageURIParamServerKey = spbStartPageApi.getStartPageURIParamServerKey();
						var lStartPageURIParamServerKeyValue = spbStartPageApi.getURIParamValue(lStartPageURIParamServerKey);
						
						if(lStartPageURIParamServerKeyValue && typeof(lStartPageURIParamServerKeyValue) == "string" && "ERROR" != lStartPageURIParamServerKeyValue.toUpperCase()) {
							var lIsNeedToResetCloudDomainURL = false;
							if("TEST" == lStartPageURIParamServerKeyValue.toUpperCase()) {
								var lStartPageCloudDomainURL = "https://testpcb.cadence.com";
								lIsNeedToResetCloudDomainURL = true;
							}
							else if("STAGE" == lStartPageURIParamServerKeyValue.toUpperCase()) {
								var lStartPageCloudDomainURL = "https://stagepcb.cadence.com";
								lIsNeedToResetCloudDomainURL = true;
							}
							
							if(lIsNeedToResetCloudDomainURL) {
								var lstartPageControlURL = lStartPageCloudDomainURL + "/startpage";
								spbStartPageApi.setStartPageControlURL(lstartPageControlURL);
								spbStartPageApi.setStartPageCloudDomainURL(lStartPageCloudDomainURL);
							}
						}
					}
				}
				
				var lStartPageCloudDomainURL = spbStartPageApi.getStartPageCloudDomainURL();
				loadHotJar(lStartPageCloudDomainURL);
				
				var lStartPageInstallMode = spbStartPageApi.getStartPageToolInstallMode();				
				if(lStartPageInstallMode && "" != lStartPageInstallMode && "VIEWER_INSTALL" == lStartPageInstallMode.toUpperCase()) {
					$("#btn_new").hide();
					$("#latest_Version").hide();
				}
				
				initClickFunctions();	
				orcadstartpagerecentfiles.init();	
				
				
				if(lStartPageInstallMode && "" != lStartPageInstallMode && "VIEWER_INSTALL" == lStartPageInstallMode.toUpperCase()) 
				{		
					spbStartPageVersion.initOffline();
				} else {
					spbStartPageVersion.init();
				}
				spbStartPageAnnouncements.init();
				spbStartPageChannelPartnerInfos.init();
				break;
		}	
	}

	function initClickFunctions() {
		$("#bodyDiv").keydown(function(p_objEvent){ handleKeyDownEvent(p_objEvent); });
		
		var lStartPageInstallMode = spbStartPageApi.getStartPageToolInstallMode();
		if(!(lStartPageInstallMode && "" != lStartPageInstallMode && "VIEWER_INSTALL" == lStartPageInstallMode.toUpperCase())) {
			$("#btn_new").click(function(){ spbStartPageApi.newDocument("btn_new"); });
		}
							
		$("#btn_open").click(function(){ spbStartPageApi.openDocument("btn_open"); });
		
		var lStartPageCloudDomainURL = spbStartPageApi.getStartPageCloudDomainURL();
		var lStartPageBucketName = spbStartPageApi.getStartPageBucketName();
		
		var orcadLinkInfo= orCADCaptureLinkInfo.gotoOrCad;	
		if (orcadLinkInfo != null && orcadLinkInfo != undefined && orcadLinkInfo.indexOf(lStartPageBucketName) == 0) {		
			//console.log("orcadLinkInfo is wih bucketUrl =" + orcadLinkInfo);
			orcadLinkInfo = lStartPageCloudDomainURL + "/" + orcadLinkInfo;		
		}
		
		var videoWalkthroughLinkInfo= orCADCaptureLinkInfo.videoWalkthrough;
		if (videoWalkthroughLinkInfo != null && videoWalkthroughLinkInfo != undefined && videoWalkthroughLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("videoWalkthroughLinkInfo is wih bucketUrl =" + videoWalkthroughLinkInfo);
			videoWalkthroughLinkInfo = lStartPageCloudDomainURL + "/" + videoWalkthroughLinkInfo;
		}
		
		var keyboardStartingGuideLinkInfo= orCADCaptureLinkInfo.keyboardStartingGuide;
		if (keyboardStartingGuideLinkInfo != null && keyboardStartingGuideLinkInfo != undefined && keyboardStartingGuideLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("keyboardStartingGuideLinkInfo is wih bucketUrl =" + keyboardStartingGuideLinkInfo);
			keyboardStartingGuideLinkInfo = lStartPageCloudDomainURL + "/" + keyboardStartingGuideLinkInfo;
		}
		
		var migrationGuideLinkInfo= orCADCaptureLinkInfo.migrationGuide;
		if (migrationGuideLinkInfo != null && migrationGuideLinkInfo != undefined && migrationGuideLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("migrationGuideLinkInfo is wih bucketUrl =" + migrationGuideLinkInfo);
			migrationGuideLinkInfo = lStartPageCloudDomainURL + "/" + migrationGuideLinkInfo;
		}
		
		var downloadDemoDesignsLinkInfo= orCADCaptureLinkInfo.downloadDemoDesigns;
		if (downloadDemoDesignsLinkInfo != null && downloadDemoDesignsLinkInfo != undefined && downloadDemoDesignsLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("downloadDemoDesignsLinkInfo is wih bucketUrl =" + downloadDemoDesignsLinkInfo);
			downloadDemoDesignsLinkInfo = lStartPageCloudDomainURL + "/" + downloadDemoDesignsLinkInfo;
		}
		
		var tutorialsLinkInfo= orCADCaptureLinkInfo.tutorials;
		if (tutorialsLinkInfo != null && tutorialsLinkInfo != undefined && tutorialsLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("tutorialsLinkInfo is wih bucketUrl =" + tutorialsLinkInfo);
			tutorialsLinkInfo = lStartPageCloudDomainURL + "/" + tutorialsLinkInfo;
		}
		
		var partLibraryLinkInfo= orCADCaptureLinkInfo.partLibrary;
		if (partLibraryLinkInfo != null && partLibraryLinkInfo != undefined && partLibraryLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("partLibraryLinkInfo is wih bucketUrl =" + partLibraryLinkInfo);
			partLibraryLinkInfo = lStartPageCloudDomainURL + "/" + partLibraryLinkInfo;
		}		
		
		var moreManualOrGuideLinkInfo= orCADCaptureLinkInfo.moreManualOrGuide;
		if (moreManualOrGuideLinkInfo != null && moreManualOrGuideLinkInfo != undefined && moreManualOrGuideLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("moreManualOrGuideLinkInfo is wih bucketUrl =" + moreManualOrGuideLinkInfo);
			moreManualOrGuideLinkInfo = lStartPageCloudDomainURL + "/" + moreManualOrGuideLinkInfo;
		}
		
		var moreDesignResourcesLinkInfo= orCADCaptureLinkInfo.moreDesignResources;
		if (moreDesignResourcesLinkInfo != null && moreDesignResourcesLinkInfo != undefined && moreDesignResourcesLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("moreDesignResourcesLinkInfo is wih bucketUrl =" + moreDesignResourcesLinkInfo);
			moreDesignResourcesLinkInfo = lStartPageCloudDomainURL + "/" + moreDesignResourcesLinkInfo;
		}		
		
		var cadenceLinkedInLinkInfo= orCADCaptureLinkInfo.cadenceLinkedIn;
		if (cadenceLinkedInLinkInfo != null && cadenceLinkedInLinkInfo != undefined && cadenceLinkedInLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("cadenceLinkedInLinkInfo is wih bucketUrl =" + cadenceLinkedInLinkInfo);
			cadenceLinkedInLinkInfo = lStartPageCloudDomainURL + "/" + cadenceLinkedInLinkInfo;		
		}
		
		var cadenceFacebookLinkInfo= orCADCaptureLinkInfo.cadenceFacebook;
		if (cadenceFacebookLinkInfo != null && cadenceFacebookLinkInfo != undefined && cadenceFacebookLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("cadenceFacebookLinkInfo is wih bucketUrl =" + cadenceFacebookLinkInfo);
			cadenceFacebookLinkInfo = lStartPageCloudDomainURL + "/" + cadenceFacebookLinkInfo;		
		}
		
		var cadenceYouTubeLinkInfo= orCADCaptureLinkInfo.cadenceYouTube;
		if (cadenceYouTubeLinkInfo != null && cadenceYouTubeLinkInfo != undefined && cadenceYouTubeLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("cadenceYouTubeLinkInfo is wih bucketUrl =" + cadenceYouTubeLinkInfo);
			cadenceYouTubeLinkInfo = lStartPageCloudDomainURL + "/" + cadenceYouTubeLinkInfo;		
		}
		
		var cadenceBlogLinkInfo= orCADCaptureLinkInfo.cadenceBlog;
		if (cadenceBlogLinkInfo != null && cadenceBlogLinkInfo != undefined && cadenceBlogLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("cadenceBlogLinkInfo is wih bucketUrl =" + cadenceBlogLinkInfo);
			cadenceBlogLinkInfo = lStartPageCloudDomainURL + "/" + cadenceBlogLinkInfo;		
		}		
		
		var moreAboutSupportLinkInfo= orCADCaptureLinkInfo.moreAboutSupport;
		if (moreAboutSupportLinkInfo != null && moreAboutSupportLinkInfo != undefined && moreAboutSupportLinkInfo.indexOf(lStartPageBucketName) == 0) {
			//console.log("moreAboutSupportLinkInfo is wih bucketUrl =" + moreAboutSupportLinkInfo);
			moreAboutSupportLinkInfo = lStartPageCloudDomainURL + "/" + moreAboutSupportLinkInfo;		
		}
		
		$("#link_gotoOrCad").click(function(){ spbStartPageApi.openURLinBrowser(orcadLinkInfo,"link_gotoOrCad"); });
		$("#link_video").click(function(){ spbStartPageApi.openURLinBrowser(videoWalkthroughLinkInfo,"link_video"); });	
		$("#link_keyBoardShortcutGuide").click(function(){ spbStartPageApi.openURLinBrowser(keyboardStartingGuideLinkInfo,"link_keyBoardShortcutGuide"); });
		$("#link_migrationGuide").click(function(){ spbStartPageApi.openURLinBrowser(migrationGuideLinkInfo,"link_migrationGuide"); });
		$("#link_dwnldDemoDesigns").click(function(){ spbStartPageApi.openURLinBrowser(downloadDemoDesignsLinkInfo,"link_dwnldDemoDesigns"); });
		$("#link_tutorials").click(function(){ spbStartPageApi.openURLinBrowser(tutorialsLinkInfo,"link_tutorials"); });
		$("#link_partLib").click(function(){ spbStartPageApi.openURLinBrowser(partLibraryLinkInfo,"link_partLib"); });
		$("#btn_moreGuide").click(function(){ spbStartPageApi.openURLinBrowser(moreManualOrGuideLinkInfo,"btn_moreGuide"); });
		$("#btn_designResource").click(function(){ spbStartPageApi.openURLinBrowser(moreDesignResourcesLinkInfo,"btn_designResource"); });
		$("#btn_linkedIn").click(function(){ spbStartPageApi.openURLinBrowser(cadenceLinkedInLinkInfo,"btn_linkedIn"); });
		$("#btn_faceBook").click(function(){ spbStartPageApi.openURLinBrowser(cadenceFacebookLinkInfo,"btn_faceBook"); });
		$("#btn_youTube").click(function(){ spbStartPageApi.openURLinBrowser(cadenceYouTubeLinkInfo,"btn_youTube"); });
		$("#btn_blog").click(function(){ spbStartPageApi.openURLinBrowser(cadenceBlogLinkInfo,"btn_blog"); });	
		$("#btn_aboutSupport").click(function(){ spbStartPageApi.openURLinBrowser(moreAboutSupportLinkInfo,"btn_aboutSupport"); });			
	}

	function handleKeyDownEvent(p_objEvent) {	
		//alert("key event came");
		var l_isCtrlKeyPressed = false;
			
		if (p_objEvent.ctrlKey) {
			l_isCtrlKeyPressed = true;
		}
		
		if(l_isCtrlKeyPressed == true) {
			switch(p_objEvent.which) {
				case OrDzGlobalEnum.KeyCode.O:
				{
					//console.log("enter to open existing document");
					spbStartPageApi.openDocument("key_CtrlPlusO");
				}
				break;

				case OrDzGlobalEnum.KeyCode.N:
				{
					var lStartPageInstallMode = spbStartPageApi.getStartPageToolInstallMode();
					if(!(lStartPageInstallMode && "" != lStartPageInstallMode 
						&& "VIEWER_INSTALL" == lStartPageInstallMode.toUpperCase())) 
					{
						//console.log("enter to create new document");
						spbStartPageApi.newDocument("key_CtrlPlusN");
					}
				}
				break;
				
				default:
				break;				
			}	
		}
	}	

}(jQuery));
 
