			var l_arParam =[event,ui,l_strDivId,orGetCustomAppParamOnId(l_strDivId,l_strEventType)];			
			orGetCustomEventHandlerOnId(l_strDivId,l_strEventType).apply(orGetCustomContextObjOnId(l_strDivId,l_strEventType),l_arParam);
		}
	}
}

