	spbStartPageApi.getStartPageToolIsAnalyticsEnabled = getStartPageToolIsAnalyticsEnabled;
	spbStartPageApi.setStartPageToolVersion = setStartPageToolVersion;
	spbStartPageApi.getStartPageToolVersion = getStartPageToolVersion;
	spbStartPageApi.setStartPageToolPatch = setStartPageToolPatch;
	spbStartPageApi.getStartPageToolPatch = getStartPageToolPatch;
	spbStartPageApi.setStartPageToolIsr = setStartPageToolIsr;
	spbStartPageApi.getStartPageToolIsr = getStartPageToolIsr;
	spbStartPageApi.setStartPageToolQir = setStartPageToolQir;
	spbStartPageApi.getStartPageToolQir = getStartPageToolQir;
	spbStartPageApi.setStartPageToolIsDemo = setStartPageToolIsDemo;
	spbStartPageApi.getStartPageToolIsDemo = getStartPageToolIsDemo;
	spbStartPageApi.setStartPageToolIsTrial = setStartPageToolIsTrial;
	spbStartPageApi.getStartPageToolIsTrial = getStartPageToolIsTrial;
	spbStartPageApi.setStartPageToolInstalledVersion = setStartPageToolInstalledVersion;
	spbStartPageApi.getStartPageToolInstalledVersion = getStartPageToolInstalledVersion;
	
	spbStartPageApi.getStartPageToolParamsName = getStartPageToolParamsName;
	spbStartPageApi.getStartPageToolParamsVal = getStartPageToolParamsVal;
	spbStartPageApi.setStartPageToolParamsVal = setStartPageToolParamsVal;
	
	spbStartPageApi.getMaxRssFeedDisplayLength = getMaxRssFeedDisplayLength;
	spbStartPageApi.setMaxRssFeedDisplayLength = setMaxRssFeedDisplayLength;
		
	spbStartPageApi.newDocument = newDocument;
	spbStartPageApi.openDocument = openDocument;
	spbStartPageApi.openDesign = openDesign;
	spbStartPageApi.openURLinBrowser = openURLinBrowser;
	spbStartPageApi.getNthIndexOf = getNthIndexOf;
	spbStartPageApi.parseFileName = parseFileName;
	spbStartPageApi.getClientApiKey = getClientApiKey;
	spbStartPageApi.preloadImages = preloadImages;
	spbStartPageApi.getURIParamValue = getURIParamValue;
	spbStartPageApi.getStartPageURIParamLocalKey = getStartPageURIParamLocalKey;
	spbStartPageApi.getStartPageIsURIParamLocalKey = getStartPageIsURIParamLocalKey;
	spbStartPageApi.setStartPageIsURIParamLocalKey = setStartPageIsURIParamLocalKey;
	spbStartPageApi.getStartPageURIParamServerKey = getStartPageURIParamServerKey;
	
	//Note startPageClientApi is generic namespace for clientApi Interfaces and will be differently handled for every product team in there productclientApi named file
	
	//function - create new document
	function newDocument(p_strKey,pCallback) {
		startPageClientApi.newDocument(p_strKey,pCallback);
	}    
	
	//function - open a dialog to open an existing Document
	function openDocument(p_strKey,pCallback) {
		startPageClientApi.openDocument(p_strKey,pCallback);
	}
	
	//function - open an design with given design path
	function openDesign(p_strDesignName,pCallback) {
		startPageClientApi.openDesign(p_strDesignName,pCallback);
	}
	
	//function - opens a URL in a browser window
	//@inURL - the URL to attempt to open
	function openURLinBrowser(inURL,p_strID,pCallback) {
		var lbStartPageisURIParamLocalKey = spbStartPageApi.getStartPageIsURIParamLocalKey();
		
		if(lbStartPageisURIParamLocalKey) {
			window.open(inURL);
		}
		else {
			startPageClientApi.openURLinBrowser(inURL,p_strID,pCallback);
		}
	}
	
	//function - a version of String.indexof that gets the Nth position of a needle (string) in a haystack (string)
	//@inHaystack - a string to look in for the inNeedle string
	//@inNeedle - a string to look for in the inHaystack string
	//@inNPos - the Nth iteration of inNeedle to look for
	//return - the position of where the Nth iteration can be found
	function getNthIndexOf(inHaystack, inNeedle, inNPos) {
		return inHaystack.split(inNeedle, inNPos).join(inNeedle).length;
	}
	
	//function - get just the file name from a full pathed file name string
	//@inString - the full pathed file name string to get the file name out of
	//return - the file name string
	function parseFileName(inString) {	
		var fileName = inString.substring(inString.lastIndexOf("\\") + 1);
		return fileName;
	}
	
	//function - get the client api key for cloud call
	//return - a stringify json of the client api key
	function getClientApiKey() {
		var gClientId = "MGY3MmYyOGEtMTRmMC00NWEyLWJkYTMtMzA4MGQ3MmM5YWQx";
		var date = new Date().toUTCString();
		var id = {};
		id['client'] = gClientId;
		id['timeStamp'] = date;
		
		var client = {};
		client['clientid'] = btoa(JSON.stringify(id));
		
		return JSON.stringify(client);
	}
	
	//function - attempt to preload images to alleviate "blinking" on rollovers
	//@inImages - an array of images to preload
	function preloadImages(inImages) {
		var images = new Array()
		for (var i = 0; i < inImages.length; i++) {
			images[i] = new Image();
			images[i].src = inImages[i];
		}
	}
	
