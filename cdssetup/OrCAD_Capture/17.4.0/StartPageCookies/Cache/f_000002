/** @constructor */
function OrDzSMI() {
}

OrDzSMI.MessageBoxType = {
    MB_ABORTRETRYIGNORE: 0x00000002,
    MB_CANCELTRYCONTINUE: 0x00000006,
    MB_HELP: 0x00004000,
    MB_OK: 0x00000000,
    MB_OKCANCEL: 0x00000001,
    MB_RETRYCANCEL: 0x00000005,
    MB_YESNO: 0x00000004,
    MB_YESNOCANCEL: 0x00000003
};

OrDzSMI.MessageBoxIcon = {
    MB_ICONWARNING: 0x00000030,
    MB_ICONASTERISK:0x00000040,
    MB_ICONQUESTION: 0x00000020,
    MB_ICONERROR: 0x00000010
};

OrDzSMI.MessageButtons = {
    MB_ABORTRETRYIGNORE: ["Abort", "Retry", "Ignore"],
    MB_CANCELTRYCONTINUE: ["Cancel", "Try Again", "Continue"],
    MB_HELP: ["Help"],
    MB_OK: ["OK"],
    MB_OKCANCEL: ["OK","Cancel"],
    MB_RETRYCANCEL: ["Retry", "Cancel"],
    MB_YESNO: ["Yes", "No"],
    MB_YESNOCANCEL: ["Yes", "No","Cancel"]
};

OrDzSMI.ButtonID = {
    BTN_OK: "OK",
    BTN_ABORT: "ABORT",
    BTN_RETRY: "RETRY",
    BTN_IGNORE: "IGNORE",
    BTN_CANCEL: "CANCEL",
    BTN_TRYAGAIN: "TRY AGAIN",
    BTN_CONTINUE: "CONTINUE",
    BTN_HELP: "HELP",
    BTN_YES: "YES",
    BTN_NO: "NO"
};

OrDzSMI.ButtonNLSID = {
    BTN_OK: 'ButtonSMIOK',
    BTN_ABORT: 'ButtonSMIAbort',
    BTN_RETRY: 'ButtonSMIRetry',
    BTN_IGNORE: 'ButtonSMIIgnore',
    BTN_CANCEL: 'ButtonSMICancel',
    BTN_TRYAGAIN: 'ButtonSMITryAgain',
    BTN_CONTINUE: 'ButtonSMIContinue',
    BTN_HELP: 'ButtonSMIHelp',
    BTN_YES: 'ButtonSMIYes',
    BTN_NO: 'ButtonSMINo'
};


OrDzSMI.sGetSMIMessage = function (pId, pModule, pArrArguments, p_objClosure) {
    
	if(pArrArguments === undefined || pArrArguments === null)
	{
		pArrArguments = [];
	}
	
	var nArguments = pArrArguments.length;
	
	switch(nArguments)
	{
		case 0:
			var lCommand = "orPrmCommonUtil::getSMIMessage {" + pId + "} {" + pModule + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 1:
			var lCommand = "orPrmCommonUtil::getSMIMessage1 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 2:
			var lCommand = "orPrmCommonUtil::getSMIMessage2 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "} {" + pArrArguments[1] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 3:
			var lCommand = "orPrmCommonUtil::getSMIMessage3 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "} {" + pArrArguments[1] + "} {" + pArrArguments[2] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 4:
			var lCommand = "orPrmCommonUtil::getSMIMessage4 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "} {" + pArrArguments[1] + "} {" + pArrArguments[2] + "} {" + pArrArguments[3] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 5:
			var lCommand = "orPrmCommonUtil::getSMIMessage5 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "} {" + pArrArguments[1] + "} {" + pArrArguments[2] + "} {" + pArrArguments[3] + "} {" + pArrArguments[4] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 6:
			var lCommand = "orPrmCommonUtil::getSMIMessage6 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "} {" + pArrArguments[1] + "} {" + pArrArguments[2] + "} {" + pArrArguments[3] + "} {" + pArrArguments[4] + "} {" + pArrArguments[5] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 7:
			var lCommand = "orPrmCommonUtil::getSMIMessage7 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "} {" + pArrArguments[1] + "} {" + pArrArguments[2] + "} {" + pArrArguments[3] + "} {" + pArrArguments[4] + "} {" + pArrArguments[5] + "} {" + pArrArguments[6] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 8:
			var lCommand = "orPrmCommonUtil::getSMIMessage8 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "} {" + pArrArguments[1] + "} {" + pArrArguments[2] + "} {" + pArrArguments[3] + "} {" + pArrArguments[4] + "} {" + pArrArguments[5] + "} {" + pArrArguments[6] + "} {" + pArrArguments[7] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 9:
			var lCommand = "orPrmCommonUtil::getSMIMessage9 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "} {" + pArrArguments[1] + "} {" + pArrArguments[2] + "} {" + pArrArguments[3] + "} {" + pArrArguments[4] + "} {" + pArrArguments[5] + "} {" + pArrArguments[6] + "} {" + pArrArguments[7] + "} {" + pArrArguments[8] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		case 10:
			var lCommand = "orPrmCommonUtil::getSMIMessage10 {" + pId + "} {" + pModule + "} {" + pArrArguments[0] + "} {" + pArrArguments[1] + "} {" + pArrArguments[2] + "} {" + pArrArguments[3] + "} {" + pArrArguments[4] + "} {" + pArrArguments[5] + "} {" + pArrArguments[6] + "} {" + pArrArguments[7] + "} {" + pArrArguments[8] + "} {" + pArrArguments[9] + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
			break;
		break;	
	}
	
};

OrDzSMI.sShowSMIMessage = function ( pId, pModule, pTitle, pType, pArrArguments, p_objClosure) {
    OrDzSMI.sShowSMIMessageInCB(0, pId, pModule, pTitle, pType, pArrArguments, p_objClosure);
};

OrDzSMI.sShowSMIMessageInCB = function ( p_nCBStage, pId, pModule, pTitle, pType, pArrArguments, p_objClosure, pMessage) {
	
	switch(p_nCBStage)
	{
		case 0:
			var lObjClosure = OrDzCallBackManager.createClosure(null, OrDzSMI.sShowSMIMessageInCB, [1, pId, pModule, pTitle, pType, pArrArguments, p_objClosure]);
			OrDzSMI.sGetSMIMessage(pId, pModule, pArrArguments, lObjClosure);
			break;
			
		case 1:
			var lCommand = "orPrmCommonUtil::showSMIMessage {" + pMessage + "} {" + pTitle + "} {" + pType + "}";
			OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", lCommand, p_objClosure);
	}
	
}

OrDzSMI.sShowSMIMessageJS = function (pId, pModule, pTitle, pMessageType, pMessageIcon, pArrArguments, p_objClosure) {
    OrDzSMI.sShowSMIMessageJSInCB(0, pId, pModule, pTitle, pMessageType, pMessageIcon, pArrArguments, p_objClosure);
};

OrDzSMI.sShowSMIMessageJSInCB = function (p_nCBStage, pId, pModule, pTitle, pMessageType, pMessageIcon, pArrArguments, p_objClosure, pMessage, p_objInputJson) {
    var l_eFuncRetStatus = { val: OrDzGlobalEnum.StatusCode.OK };
    switch (p_nCBStage) {
        case 0:
            OrDzCmdDivMgr.startProgress();
            OrDzCmdDivMgr.addProgress(l_eFuncRetStatus, 20);

            var lObjClosure = OrDzCallBackManager.createClosure(null, OrDzSMI.sShowSMIMessageJSInCB, [p_nCBStage + 1, pId, pModule, pTitle, pMessageType, pMessageIcon, pArrArguments, p_objClosure]);
            OrDzSMI.sGetSMIMessage(pId, pModule, pArrArguments, lObjClosure);
            break;
        case 1:
            var lObjClosure = OrDzCallBackManager.createClosure(null, OrDzSMI.sShowSMIMessageJSInCB, [p_nCBStage + 1, pId, pModule, pTitle, pMessageType, pMessageIcon, pArrArguments, p_objClosure, pMessage]);
            var l_strTclCommand = "::OrEntrySymbolEditorConfig::WriteToSessionLog " + "{" + pMessage + "}";
            OrDzServerInterface.sGetServerInterface().nativeCall("executeTcl", l_strTclCommand, lObjClosure);
            //OrDzServerCall.sGetServerCall(l_eFuncRetStatus).WriteToSessionLog(l_eFuncRetStatus, lObjClosure, pMessage);
            break;
        case 2:

            var l_nMessageButton = null;
            
            for (l_nButtonKey in OrDzSMI.MessageBoxType) {
                if (pMessageType == OrDzSMI.MessageBoxType[l_nButtonKey]) {
                    l_nMessageButton = l_nButtonKey;
                    break;
                }
            };
            if (l_nMessageButton == null) {
                var l_arButtons = OrDzSMI.MessageButtons.MB_OK;
            } else {
                var l_arButtons = OrDzSMI.MessageButtons[l_nMessageButton];
            }
            var l_arButtonDisplayText = [];
            for (var l_nIndex = 0; l_nIndex < l_arButtons.length; ++l_nIndex) {
                for (key in OrDzSMI.ButtonID) {
                    if (OrDzSMI.ButtonID[key] == l_arButtons[l_nIndex].toUpperCase()) {
                        l_arButtonDisplayText.push(window['i18nOrcad'][OrDzSMI.ButtonNLSID[key]]);
                    }
                }
            };
            var l_strClass = "smiIconMsgError";
            switch (pMessageIcon) {
                case OrDzSMI.MessageBoxIcon.MB_ICONWARNING:
                    l_strClass = "smiIconMsgWarning";
                    break;
                case OrDzSMI.MessageBoxIcon.MB_ICONASTERISK:
                    l_strClass = "smiIconMsgInformation";
                    break;
                case OrDzSMI.MessageBoxIcon.MB_ICONQUESTION:
                    l_strClass = "smiIconMsgQuestion";
                    break;
                case OrDzSMI.MessageBoxIcon.MB_ICONERROR:
                    l_strClass = "smiIconMsgError";
                    break;
            };           

            var lJson = {
                "Name": "SMIMessage",
                "Sections": [
					{
					    "Type": "Normal",
					    "Name": "Section1",
					    "Sectionheight": 90,
					    "Panels": [
							{
							    "Type": "Normal",
							    "Name": "Message",
							    "collapsible": false,
							    "Rows": [
									{
									    "class": "tablewidth", "Cells": [
                                               { "Type": "TEXT", "class": l_strClass, "InputValue": "", "Title": "", "Align": "Left" },
                                               { "Type": "TEXT", "class": "", "InputValue": pMessage, "Title": "" }

									    ]
									}
							    ]
							}, {
							    "Type": "Normal",
							    "Name": "Buttons",
                                "FieldSetClass" : "smiButtonactionArea",
							    "collapsible": false
							}
					    ]
					}
                ]
            };
            
            var l_fnClsoureButtonClick = OrDzCallBackManager.createClosure(OrDzSMI, OrDzSMI.sButtonClicked, [p_objClosure]);

            var l_objRow = {};
            l_objRow["Cells"] = [];
            for (var l_nIndex = 0; l_nIndex < l_arButtonDisplayText.length; ++l_nIndex) {
                var l_strCallBackMethodKey = 'SmiButtonClicked'+l_nIndex;
                l_objRow["Cells"].push({ "Type": "Button", "class": "", "Disabled": false, "CallBackMethod": { "onButtonClick": [l_strCallBackMethodKey, l_fnClsoureButtonClick] }, "cellClass": "AttrBFormBtn", "Title": l_arButtonDisplayText[l_nIndex], "InputValue": l_arButtonDisplayText[l_nIndex] });
            };
            l_objRow["AttrCustom"] = { "customOptions": { 'name': "Options", 'data': l_arButtons } };
            lJson["Sections"][0]["Panels"][1]["Rows"] = [];
            lJson["Sections"][0]["Panels"][1]["Rows"].push(l_objRow);
            
            var l_strLayout = "<div data-dojo-type=\"dijit.layout.ContentPane\" style=\"padding:0px;height:100%;\" id=\"Show_SMI_Message\" ></div>";
            var l_closure = OrDzCallBackManager.createClosure(OrDzSMI, OrDzSMI.onClickSMIClose, [p_objClosure]);
            var l_closureDijitCreation = OrDzCallBackManager.createClosure(OrDzSMI, OrDzSMI.sShowSMIMessageJSInCB, [p_nCBStage + 1, pId, pModule, pTitle, pMessageType, pMessageIcon, pArrArguments, p_objClosure, pMessage, lJson]);
            OrDzDojo.dijitDialog(pTitle, l_strLayout, l_closure, "width:40em;height:auto", true, l_closureDijitCreation, null, OrDzGlobalEnum.DialogDivID.SMIMessageShowDialog);
            break;
        case 3:
            OrDzCmdDivMgr.addProgress(l_eFuncRetStatus, 80);
            OrDzCmdDivMgr.endProgress();
            OrDzSession.sGetSession(l_eFuncRetStatus).getDivManager(l_eFuncRetStatus).setDialogEventLoop(l_eFuncRetStatus, true);
            var l_MainDiv = document.getElementById("Show_SMI_Message");
			//note SMI message in JS(JavaScript) needs OrAttributeForm and in case OrAttributeForm in not loaded, then no need to include below code to avoid errors
			if("undefined" != typeof OrAttributeForm) {
				OrAttributeForm.sCreateForm(OrSMIMessageForm, p_objInputJson, "Show_SMI_Message", null);  
			}				
            break;
    }
};

OrDzSMI.sButtonClicked = function (p_objClosure, p_objJson) {
    var l_eFuncRetStatus = { val: OrDzGlobalEnum.StatusCode.OK };    
    var lSectionIndex = p_objJson["SectionIndex"];
    var lPanelIndex = p_objJson["PanelIndex"];
    var lRowIndex = p_objJson["RowIndex"];
    var lCellIndex = p_objJson["CellIndex"];
    var l_strButtonClicked = p_objJson["Json"]["Sections"][lSectionIndex]["Panels"][lPanelIndex]["Rows"][lRowIndex]["AttrCustom"]["Options"]["data"][lCellIndex];
    OrDzDojo.dijitDialogDestroy(l_eFuncRetStatus, OrDzGlobalEnum.DialogDivID.SMIMessageShowDialog);
    OrDzSession.sGetSession(l_eFuncRetStatus).getDivManager(l_eFuncRetStatus).setDialogEventLoop(l_eFuncRetStatus, false);    

    if (p_objClosure != null && p_objClosure != undefined) {
        p_objClosure(l_strButtonClicked);
    }

};

OrDzSMI.onClickSMIClose = function (p_objClosure) {
    var l_eFuncRetStatus = { val: OrDzGlobalEnum.StatusCode.OK };
    OrDzDojo.dijitDialogDestroy(l_eFuncRetStatus, OrDzGlobalEnum.DialogDivID.SMIMessageShowDialog);
    OrDzSession.sGetSession(l_eFuncRetStatus).getDivManager(l_eFuncRetStatus).setDialogEventLoop(l_eFuncRetStatus, false);
    if (p_objClosure != null && p_objClosure != undefined) {
        p_objClosure("Cancel");
    }    
};

//note SMI message in JS(JavaScript) needs OrAttributeForm and in case OrAttributeForm in not loaded, then no need to include below code to avoid errors
if("undefined" != typeof OrAttributeForm) {
	/** @constructor */
	function OrSMIMessageForm() {
		OrAttributeForm.call(this);
		this.mObjOwner = null;
	}

	OrSMIMessageForm.prototype = new OrAttributeForm();
	OrSMIMessageForm.prototype.Constructor = OrSMIMessageForm;
}
 
var l_objEventHandlerOnIdJson ={};

function orCheckIsValid (p_objElement) {
	var l_bValid = false;
	if(p_objElement != null && p_objElement != undefined) {
		l_bValid = true;
	}
	return l_bValid;
}

function orKeepEventHandlerOnId (p_strDivId, p_strEventType,p_fnHandler,p_objThis,p_objAppDataJson) {
	if(orCheckIsValid(l_objEventHandlerOnIdJson[p_strDivId])) {
		l_objEventHandlerOnIdJson[p_strDivId][p_strEventType] = {"HandlerRef" :p_fnHandler,"ContextObj":p_objThis,"DivId":p_strDivId,"AppParam":p_objAppDataJson};	
	}
	else {
		l_objEventHandlerOnIdJson[p_strDivId] = {};
		l_objEventHandlerOnIdJson[p_strDivId][p_strEventType] = {"HandlerRef": p_fnHandler,"ContextObj":p_objThis,"DivId":p_strDivId,"AppParam":p_objAppDataJson};
	}	
}

function orGetCustomEventHandlerOnId (p_strDivId,p_strEventType) {
	if(orCheckIsValid(p_strDivId) && orCheckIsValid(l_objEventHandlerOnIdJson[p_strDivId]) && orCheckIsValid(l_objEventHandlerOnIdJson[p_strDivId][p_strEventType])) {
		return l_objEventHandlerOnIdJson[p_strDivId][p_strEventType]["HandlerRef"]
	}
	else {
		return null;
	}
}

function orGetCustomAppParamOnId (p_strDivId,p_strEventType) {
	if(orCheckIsValid(p_strDivId) && orCheckIsValid(l_objEventHandlerOnIdJson[p_strDivId]) && orCheckIsValid(l_objEventHandlerOnIdJson[p_strDivId][p_strEventType])) {
		return l_objEventHandlerOnIdJson[p_strDivId][p_strEventType]["AppParam"]
	}
	else {
		return null;
	}
}

function orGetCustomContextObjOnId (p_strDivId,p_strEventType) {
	if(orCheckIsValid(p_strDivId) && orCheckIsValid(l_objEventHandlerOnIdJson[p_strDivId]) && orCheckIsValid(l_objEventHandlerOnIdJson[p_strDivId][p_strEventType])) {
		return l_objEventHandlerOnIdJson[p_strDivId][p_strEventType]["ContextObj"]
	}
	else {
		return null;
	}
}

function orAttachHandlerOnDiv (p_strDivId,p_strEventType,p_refHandler,p_objThis,p_objAppDataJson) {	
	if(orCheckIsValid(p_strDivId) && orCheckIsValid(p_refHandler) && orCheckIsValid(p_strEventType)) {			
		var l_objDivEle = document.getElementById(p_strDivId);
		if(orCheckIsValid(l_objDivEle)) {
			var l_strEventType = p_strEventType.toLowerCase();
			switch(l_strEventType) {
				case "click": {
					l_objDivEle.addEventListener(l_strEventType,onclick,false);//attaching generic event handler on div element for particular event type
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );//storing refrence of custom event handler to call on particular event type
				}
				break;
				
				case "dblclick": {
					l_objDivEle.addEventListener(l_strEventType,ondblclick,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "change":
				{
					l_objDivEle.addEventListener(l_strEventType,onchange,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "changecustomdropdown":
				{	
					var l_strAttachDivId = "#"+ p_strDivId;
					$(l_strAttachDivId).on( "selectmenuchange", onChangeCustomDropDown);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "blur":
				{
					l_objDivEle.addEventListener(l_strEventType,onblur,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "focus":
				{
					l_objDivEle.addEventListener(l_strEventType,onfocus,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "focusout":
				{
					l_objDivEle.addEventListener(l_strEventType,onfocusout,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "keyup":					
				{
					l_objDivEle.addEventListener(l_strEventType,onkeyup,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "keydown":					
				{
					l_objDivEle.addEventListener(l_strEventType,onkeydown,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "keypress":					
				{
					l_objDivEle.addEventListener(l_strEventType,onkeypress,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "mouseup":					
				{
					l_objDivEle.addEventListener(l_strEventType,onmouseup,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "mousedown":					
				{
					l_objDivEle.addEventListener(l_strEventType,onmousedown,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "mousemove":					
				{
					l_objDivEle.addEventListener(l_strEventType,onmousemove,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "mouseover":					
				{
					l_objDivEle.addEventListener(l_strEventType,onmouseover,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "mouseout":					
				{
					l_objDivEle.addEventListener(l_strEventType,onmouseout,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "mouseenter":					
				{
					l_objDivEle.addEventListener(l_strEventType,onmouseenter,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				case "mouseleave":					
				{
					l_objDivEle.addEventListener(l_strEventType,onmouseleave,false);
					orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );
				}
				break;
				
				default:
				break;					
			}																																				
		}
	}
}

function orAttachEventsOnTree (p_strRootDivId,p_strEventType) {
	if(orCheckIsValid(p_strRootDivId) && orCheckIsValid(p_strEventType)) {
		var l_strEventType = p_strEventType.toLowerCase();
		switch(l_strEventType) {
			case "select_node": {
				var l_strAttachEvnetType = l_strEventType+".jstree";
				var l_strAttachRootDivId = "#"+ p_strRootDivId;
				//$('#wrapper').on(l_strAttachEvnetType, onSelectNode);						
				$(l_strAttachRootDivId).on(l_strAttachEvnetType, onSelectNode);	//attaching generic event handler on root of tree for particular event type									
			}
			break;
								
			default:
			break;			
		}							
	}
}

function orAttachHandlerOnTreeDivId (p_strRootDivId,p_strDivId,p_strEventType,p_refHandler,p_objThis,p_objAppDataJson) {	
	if(orCheckIsValid(p_strRootDivId) && orCheckIsValid(p_strDivId) && orCheckIsValid(p_refHandler) && orCheckIsValid(p_strEventType)) {
		var l_strEventType = p_strEventType.toLowerCase();
		switch(l_strEventType) {
			case "select_node": {					
				orKeepEventHandlerOnId(p_strDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );//storing refrence of custom event handler to call on particular event type
			}
			break;
								
			default:
			break;			
		}							
	}
}

function orAttachCustomRmbMenuOnTree (p_strRootDivId,p_strEventType,p_refHandler,p_objThis,p_objAppDataJson) {	
	if(orCheckIsValid(p_strRootDivId) && orCheckIsValid(p_refHandler) && orCheckIsValid(p_strEventType)) {
		var l_strEventType = p_strEventType.toLowerCase();
		switch(l_strEventType) {
			case "rmb_mouseclick": {			
				orKeepEventHandlerOnId(p_strRootDivId,l_strEventType,p_refHandler,p_objThis,p_objAppDataJson );//storing refrence of custom event handler to call on particular event type
			}
			break;
								
			default:
			break;			
		}							
	}
}

function orCreateCustomRmbMenuOnTree(node) {
	var l_strDivId = null;
	var l_strEventType = "rmb_mouseclick";
	var l_nNumberOfParents = node.parents.length;
	if(l_nNumberOfParents > 1) {
		l_strDivId = document.getElementById(node.parents[l_nNumberOfParents-2]).parentElement.parentElement.id;
	}
	else {
		l_strDivId = document.getElementById(node['id']).parentElement.parentElement.id;
	}
	
	if(orCheckIsValid(orGetCustomEventHandlerOnId(l_strDivId,l_strEventType))) {
			//return orGetCustomEventHandlerOnId(l_strDivId,l_strEventType)(node,l_strDivId,orGetCustomAppParamOnId(l_strDivId,l_strEventType));
			var l_arParam =[node,l_strDivId,orGetCustomAppParamOnId(l_strDivId,l_strEventType)];			
			return orGetCustomEventHandlerOnId(l_strDivId,l_strEventType).apply(orGetCustomContextObjOnId(l_strDivId,l_strEventType),l_arParam);
	}
	
}

function onSelectNode(event,data) {
	var l_strDivId = null;
	var l_strEventType = "select_node";
	if(orCheckIsValid(data.node) && orCheckIsValid(data.node.id)) {
		l_strDivId = data.node.id;
	
		if(orCheckIsValid(orGetCustomEventHandlerOnId(l_strDivId,l_strEventType))) {
			//orGetCustomEventHandlerOnId(l_strDivId,l_strEventType)(event,l_strDivId,orGetCustomAppParamOnId(l_strDivId,l_strEventType));
			var l_arParam =[event,l_strDivId,orGetCustomAppParamOnId(l_strDivId,l_strEventType)];			
			orGetCustomEventHandlerOnId(l_strDivId,l_strEventType).apply(orGetCustomContextObjOnId(l_strDivId,l_strEventType),l_arParam);
		}
	}
}

function onclick(event) {
	var l_strDivId = null;
	var l_strEventType = "click";
	if(orCheckIsValid(event['currentTarget']) && orCheckIsValid(event['currentTarget']['id'])) {
		l_strDivId = event['currentTarget']['id'];
	
		if(orCheckIsValid(orGetCustomEventHandlerOnId(l_strDivId,l_strEventType))) {
			//orGetCustomEventHandlerOnId(l_strDivId,l_strEventType)(event,l_strDivId,orGetCustomAppParamOnId(l_strDivId,l_strEventType));
			var l_arParam =[event,l_strDivId,orGetCustomAppParamOnId(l_strDivId,l_strEventType)];			
			orGetCustomEventHandlerOnId(l_strDivId,l_strEventType).apply(orGetCustomContextObjOnId(l_strDivId,l_strEventType),l_arParam);
		}
	}
}

