function orSetAppTheme(p_Callback) {
    //fetch the css variables to set
    var root = document.querySelector(':root');

    var l_strAppTheme = "dark";//default theme

    if (window['external']['QtWebEngine'] == true) {
        window['external']['orPrmAppTheme'](function (pStrResponse) {
            l_strAppTheme = pStrResponse;
            root.className = l_strAppTheme;
            if (p_Callback != null || p_Callback != undefined) {
                p_Callback();
            }
        }
		)
    }
    else if (window != undefined && window['external'] != undefined && window['external']['orPrmAppTheme'] != undefined) {

        l_strPrmAppTheme = window['external']['orPrmAppTheme'];

        if (l_strPrmAppTheme.toLowerCase() == "light") {
            l_strAppTheme = "light";
        }
        else if (l_strPrmAppTheme.toLowerCase() == "dark") {
            l_strAppTheme = "dark";
        }
        else {//default theme
            l_strAppTheme = "dark";//default theme
        }

        root.className = l_strAppTheme;
        if (p_Callback != null || p_Callback != undefined) {
            p_Callback();
        }
    }

}

function orGetAppTheme() {		
	var l_strAppTheme = "dark";//default theme
	
	if(window != undefined && window['external'] != undefined && window['external']['orPrmAppTheme'] != undefined) {
		var l_strPrmAppTheme = window['external']['orPrmAppTheme'];
		
		if(l_strPrmAppTheme.toLowerCase() == "light") {
			l_strAppTheme = "light";
		}
		else if(l_strPrmAppTheme.toLowerCase() == "dark") {
			l_strAppTheme = "dark";
		}
		else {//default theme
			l_strAppTheme = "dark";//default theme
		}
	}
	
	return l_strAppTheme;	
}

function orSetTreeAppTheme(p_strRootDivId) {
	var l_strAppTheme = "dark";//default theme
	
	if(window != undefined && window['external'] != undefined && window['external']['orPrmAppTheme'] != undefined) {
		var l_strPrmAppTheme = window['external']['orPrmAppTheme'];
		
		if(l_strPrmAppTheme.toLowerCase() == "light") {
			l_strAppTheme = "light";
		}
		else if(l_strPrmAppTheme.toLowerCase() == "dark") {
			l_strAppTheme = "dark";
		}
		else {//default theme
			l_strAppTheme = "dark";//default theme
		}
	}
	
	if(orCheckIsValid(p_strRootDivId)) {
		var l_strAttachRootDivId = "#"+ p_strRootDivId;
		
		if(l_strAppTheme == "light") {
			$(l_strAttachRootDivId).jstree("set_theme", "default");//Note in Js tree default is light, so in case of light theme,setting default as path for resources
		}
		else if(l_strAppTheme == "dark") {
			$(l_strAttachRootDivId).jstree("set_theme", "default-dark");
		}
		else {//default theme resources
			$(l_strAttachRootDivId).jstree("set_theme", "default-dark");//default theme resources
		}
	}
}

/** @constructor */
function orI18nOrcad() {
	
}

orI18nOrcad.prototype = new orI18nOrcad();  /* Inherit with the same Class */
orI18nOrcad.prototype.constructor = orI18nOrcad;

//static variable definition
orI18nOrcad.sObjLocalizeTextJson =null ;

orI18nOrcad.sLoadNlsFramework = function(p_strApplicationName,pCallbackFunction) {
	orI18nOrcad.pvt_LoadNlsFrameworkInCB(p_strApplicationName, pCallbackFunction, 0);
}

orI18nOrcad.pvt_LoadNlsFrameworkInCB = function (p_strApplicationName, pCallbackFunction, p_nCBStage)
{
	switch(p_nCBStage)
	{
		case 0:
			var l_fnClosure = OrDzCallBackManager.createClosure(null, orI18nOrcad.pvt_LoadNlsFrameworkInCB, [p_strApplicationName, pCallbackFunction, 1]);
			orI18nOrcad.sLoadNlsFile(p_strApplicationName, l_fnClosure,0);
			break;
		case 1:
			orI18nOrcad.sInitializei18NextFramework();
			if(pCallbackFunction != null && pCallbackFunction != undefined)
			{
				pCallbackFunction();
			}			
			break;
	}
}

orI18nOrcad.sLoadNlsFile = function(p_strApplicationName,pCallbackFunction,p_nCBStage) {
	switch(p_nCBStage)
	{
		case 0:
			var locale= "en-us";
			
			// Chek what is system locale
			var bIsIE = parseFloat(navigator['appVersion']['split']("MSIE ")[1]) || undefined;
			var lSystemLocale = (bIsIE ? navigator['userLanguage'] : navigator['language'])['toLowerCase']();
			
			if( undefined != lSystemLocale &&  null != lSystemLocale) {
				locale = lSystemLocale;
			}
			
			var lNlsFilePath = "../../../../../share/cdssetup/nls/";
			
			if(locale.indexOf("en") == 0) {
				lNlsFilePath = "../../../../../share/cdssetup/nls/en/";
			}
			else if(locale.indexOf("ja") == 0) {
				lNlsFilePath = "../../../../../share/cdssetup/nls/ja/";
			}
			else {
				lNlsFilePath = "../../../../../share/cdssetup/nls/en/";
			}
				
			lNlsFilePath = lNlsFilePath + p_strApplicationName + ".js";
			
			var lDomScript = document.createElement("script");
			lDomScript.setAttribute( "src" , lNlsFilePath);
			lDomScript.setAttribute("type" , "text/javascript");
			
			document.head.appendChild(lDomScript);
			
			var l_fnClosure = OrDzCallBackManager.createClosure(null, orI18nOrcad.sLoadNlsFile, [p_strApplicationName, pCallbackFunction, 1]);
			
			lDomScript.onload = function () {
				l_fnClosure();
			}
			break;

		case 1:		
			orI18nOrcad.sObjLocalizeTextJson =  l_objLocalizeTextJson ;
			if(pCallbackFunction != null && pCallbackFunction != undefined)
			{
				pCallbackFunction();
			}
			break;		
	}
}

orI18nOrcad.sInitializei18NextFramework = function() {
	i18next.init({
		lng: 'en', // evtl. use language-detector https://github.com/i18next/i18next-browser-languageDetector
		resources: { // evtl. load via xhr https://github.com/i18next/i18next-xhr-backend
			en: {
				translation: orI18nOrcad.sObjLocalizeTextJson
			}
		}
	}, function(err, t) {
		//var abc = i18next.language;
		//i18next.changeLanguage('de');
		localize = locI18next.init(i18next);
		localize('body');
	});
}

orI18nOrcad.sGetValue = function(p_nNlsId) {
	if(orI18nOrcad.sObjLocalizeTextJson[p_nNlsId] != undefined) {
		return orI18nOrcad.sObjLocalizeTextJson[p_nNlsId];
	}
	else {
		return p_nNlsId;
	}
} 
