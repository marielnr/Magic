/**
*File Name :- SPBStartPageVersion.js
*/

// create namespace object if it doesn't exist
var spbStartPageVersion = spbStartPageVersion || {};

// closure keeps everything within out of global namespace
(function ($) { "use strict";

	spbStartPageVersion.init = init;
	spbStartPageVersion.initOffline = initOffline;
	spbStartPageVersion.getVersionCurrent = getVersionCurrent;
	spbStartPageVersion.getVersionLatest = getVersionLatest;
	
	//function - run some initial functions when this is loaded
    function init() {		
		getVersionCurrent();		
		getVersionLatest();
    }
	
	//function - run some initial functions when this is loaded
    function initOffline() {	
		getVersionCurrent();		
    }

	//function - load the version from the application
	//return - the version information obtained from the tcl call
    function getVersionCurrent() {
	  	var promise = new Promise(function(resolve, reject) {
			try {
				getVersionCurrentSuccess();
			}
			catch(err) {
				//console.log("spbStartPageVersion.getVersionCurrent catch error ="+err);
				getVersionCurrentFailure(err);
			}
		});

		return promise;		
    }
	
    //function - what to do if getting the version info from the application fails
    //@inError - the error object that is created on failure
    function getVersionCurrentFailure(inError) {
    	$("#current_Version").text("");
		$("#current_Version").text("Undetermined");
    }
    
    //function - what to do if getting the version info from the application succeeds
    function getVersionCurrentSuccess() {
		var lStrInstalledVersion = spbStartPageApi.getStartPageToolInstalledVersion();
		$("#current_Version").empty();
		$("#current_Version").text(lStrInstalledVersion);	
    }
    
    //function - attempt to load the latest version info from the control site
    function getVersionLatest() {		
	  	var promise = new Promise(function(resolve, reject) {
			try {
				var lStartPageToolParamsNameJsonObj = spbStartPageApi.getStartPageToolParamsName();
				var lClientIdParam = lStartPageToolParamsNameJsonObj.ClientIdParam;
				var lClientParam = lStartPageToolParamsNameJsonObj.ClientParam;
				var lInstallModeParam = lStartPageToolParamsNameJsonObj.InstallModeParam;
				
				var lInstallMode = spbStartPageApi.getStartPageToolInstallMode();	
			
				var Latest_VersionUrl="";
				var clientParamValJsonNode = {};
				var clientParamVal="";
				
				if(null != lInstallMode && undefined != lInstallMode && "" != lInstallMode) {
					clientParamValJsonNode[lInstallModeParam] = lInstallMode;
				}
									
				var lClientApiKey= spbStartPageApi.getClientApiKey();						
				var lClientApiKeyJsonNode = JSON.parse(lClientApiKey);						
									
				clientParamValJsonNode[lClientIdParam] = lClientApiKeyJsonNode[lClientIdParam];										
				clientParamVal = JSON.stringify(clientParamValJsonNode);
				
				Latest_VersionUrl = spbStartPageApi.getStartPageControlURL() + "/latestproductversion?" + lClientParam + "=" + encodeURIComponent(clientParamVal);
				//console.log("Latest_VersionUrl ="+Latest_VersionUrl);
				OrDzHttpInterface.sGetHttpInterface().sendRequest(Latest_VersionUrl, null, showVersionLatest, null);
			}
			catch(err) {
				//console.log("spbStartPageVersion.getVersionLatest catch error ="+err);
				getVersionLatestFailure();
			}
		});

		return promise;		    	
    }
    
    //function - what to do if getting the latest version info fails
    //@inError - the error object that is created on failure
    function getVersionLatestFailure(inError) {
    	//nothing, the default "undetermined" should already be set
		//console.log("getVersionLatestFailure returned");
		$("#latest_released_Version").text("");
		$("#latest_released_Version").text("Undetermined");
		
    }    
        
	//function - display on screen the latest version info
	//@data - information regarding the latest version
	//@status - an object with information regarding how the tcl command completed
     function showVersionLatest(data, status) {
		 try {
			data = JSON.parse(data);

			var lLatestVersionString ="";
			
			 $.each(data.nodes, function(i, item) {
				 lLatestVersionString = data.nodes[0].node.field_version;			 
				 //console.log("inside showVersionLatest returned latest version is lLatestVersionString = "+lLatestVersionString);

				$("#latest_released_Version").empty();
				$("#latest_released_Version").text(lLatestVersionString);
			}); 
		 }
		 catch(err) {
			//console.log("showVersionLatest return error ="+err);
			$("#latest_released_Version").text("");
			$("#latest_released_Version").text("Undetermined");
		}
    } 
        
}(jQuery));
 
