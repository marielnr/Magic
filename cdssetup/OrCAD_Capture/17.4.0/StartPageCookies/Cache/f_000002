/*global OrDzPrmInterface*/

/** @constructor */
function OrDzHostedInterface() {
}

OrDzHostedInterface.s_objHostedInterface = null;

OrDzHostedInterface.sGetHostedInterface = function () {
    if (null === OrDzHostedInterface.s_objHostedInterface) {
        OrDzHostedInterface.s_objHostedInterface = new OrDzHostedInterface();
    }
    return OrDzHostedInterface.s_objHostedInterface;
};

OrDzHostedInterface.prototype.onAsyncResponseReceived = function (p_fClosure, p_strResponse) {
	
    ////alert("OrDzHostedInterface.prototype.onAsyncResponseReceived " + p_strResponse);
	
    if(p_fClosure !== null) {
        p_fClosure(p_strResponse);
    }
}

OrDzHostedInterface.prototype.nativeCallASync = function (p_fMethod, p_strParams, p_fClosure) {

    ////alert("OrDzHostedInterface.prototype.nativeCallASync " + p_fMethod + " " + p_strParams + " " + orPrmConnector);
   
    var lOnResponseReceivedClosureCB = OrDzCallBackManager.createClosure(this, this.onAsyncResponseReceived, [p_fClosure]);
   
    window['orPrmConnector']['setMessageCallback'](p_fMethod, lOnResponseReceivedClosureCB);
	
    window['orPrmConnector']['sendMessage'](p_fMethod, [p_strParams]);
};

OrDzHostedInterface.prototype.nativeCallSync = function (p_fMethod, p_strParams, p_fClosure) {

    ////alert("OrDzHostedInterface.prototype.nativeCallSync " + p_fMethod + " " + p_strParams);
    if(window.external['QtWebEngine'] == true)
    {
	window.external['orPrmConnector'](p_fMethod, p_strParams, function(pStrResponse)
		{
			if(p_fClosure !== null) 
			{
				p_fClosure(pStrResponse);
			}
		}
	);	

    } else {
	var l_strResponse = window.external['orPrmConnector'](p_fMethod, p_strParams);	
	
        if(p_fClosure !== null) {
            p_fClosure(l_strResponse);
        }
    }
};

OrDzHostedInterface.prototype.nativeCall = function (p_fMethod, p_strParams, p_fClosure) {

    p_strParams += " hostedClientId";
    ////alert("OrDzHostedInterface.prototype.nativeCall " + p_fMethod + " " + p_strParams);
    
    if (typeof window['orPrmConnector'] !== 'undefined') {
        this.nativeCallASync(p_fMethod, p_strParams, p_fClosure);
    } else {
        this.nativeCallSync(p_fMethod, p_strParams, p_fClosure);
    }
	
};



OrDzHostedInterface.prototype.nativeSendRequest = function (p_fMethod, p_strParams, p_fClosure, p_strURL, p_strURLParams, p_strURLMethod) {
    this.nativeCall(p_fMethod, p_strParams, p_fClosure);
};
 
/*

Copyright by Cadence Design Systems, Inc.\nAll rights reserved.

"This computer program is protected by copyright law and international treaty. 
Unauthorized reproduction or distribution of this program, or any portion of it, may result in severe civil penalties, 
and will be prosecuted to the maximum extent possible under the law."

*/

/*global OrDzBaseScript, OrDzHostedInterface,OrDzHttpInterface */

/** @constructor */
function OrDzServerInterface() {
}

OrDzServerInterface.s_strDocumentMode = "local";

OrDzServerInterface.setDocumentMode = function (p_strMode) {
    OrDzServerInterface.s_strDocumentMode = p_strMode;
};

OrDzServerInterface.getDocumentMode = function () {
    return OrDzServerInterface.s_strDocumentMode;
};

OrDzServerInterface.isHosted = function () {

    var l_bHosted = false;
    if (OrDzServerInterface.s_strDocumentMode == "local") {
        if (window.external && (window.external['orPrmWidgetId'] != null)) {
            l_bHosted = true;
        } else {
            OrDzServerInterface.s_strDocumentMode = "server";
        }
    }
    return l_bHosted;
};

OrDzServerInterface.sGetServerInterface = function () {
    if (OrDzServerInterface.isHosted()) {
        return OrDzHostedInterface.sGetHostedInterface();
    } else {
        return OrDzHttpInterface.sGetHttpInterface();
    } 
}; 
/*

Copyright by Cadence Design Systems, Inc.\nAll rights reserved.

"This computer program is protected by copyright law and international treaty. 
Unauthorized reproduction or distribution of this program, or any portion of it, may result in severe civil penalties, 
and will be prosecuted to the maximum extent possible under the law."

*/
/*global XMLHttpRequest, OrDzCallBackManager, escape*/

/** @constructor */
function OrDzHttpInterface() {
    this.m_objXHR = null;
}
OrDzHttpInterface.s_objHttpInterface = null;

OrDzHttpInterface.sGetHttpInterface = function () {
    if (null === OrDzHttpInterface.s_objHttpInterface) {
        OrDzHttpInterface.s_objHttpInterface = new OrDzHttpInterface();
    }
    return OrDzHttpInterface.s_objHttpInterface;
};

OrDzHttpInterface.prototype.getXMLHttpRequest = function () {

    if (null === this.m_objXHR) {
        this.m_objXHR = new XMLHttpRequest();
    }
    return this.m_objXHR;
};

//p_objParam in form of name value pair
OrDzHttpInterface.prototype.sendFormRequest = function (p_strUrl, p_objParam, p_strMethod) {
    var form = document.createElement("form");
    form.setAttribute("method", p_strMethod);
    form.setAttribute("action", p_strUrl);

    for(var key in p_objParam) {
        if(p_objParam.hasOwnProperty(key)) {
            var hiddenField = document.createElement("input");
            hiddenField.setAttribute("type", "hidden");
            hiddenField.setAttribute("name", key);
            hiddenField.setAttribute("value", p_objParam[key]);

            form.appendChild(hiddenField);
        }
    }

    var lCsrfToken = "";
    var lCsrfField = document['getElementById']('csrftoken');
    if (lCsrfField != null || lCsrfField != undefined) {
        lCsrfToken = lCsrfField['value'];
    }

    if (lCsrfToken != "") {
        var i = document['createElement']("input");
        i['setAttribute']('name', '_csrf');
        i['setAttribute']('value', lCsrfToken);
        form['appendChild'](i);
    }

    form.submit();

};

OrDzHttpInterface.prototype.postRequest = function (p_strType, p_strUrl, p_strParam, p_fClosure, p_nCBStage) {

    if (p_nCBStage == null || p_nCBStage == undefined) {
        p_nCBStage = 0;
    }

    var l_objThis = this;

    var lCsrfToken = "";
    var lCsrfField = document['getElementById']('csrftoken');
    if (lCsrfField != null || lCsrfField != undefined) {
        lCsrfToken = lCsrfField['value'];
    }

    switch (p_nCBStage) {
        case 0:
            $['ajax']({
                type: p_strType,
                url: p_strUrl,
                data: p_strParam,
                "beforeSend": function (xhr) { if (lCsrfToken != "") { xhr.setRequestHeader('csrf-token', lCsrfToken); } },
                "success": function (responseText) {

                    if (responseText && ((typeof responseText) == "string") && responseText.indexOf('{"serverRedirect') !== -1) {
                    //    console.log(responseText);
                        var lObjJSON = JSON.parse(responseText);
                        var lRedirectUrl = lObjJSON['serverRedirect'];
                        var lSite = window.location.protocol + "//" + window.location.hostname + lRedirectUrl;
                        window.location.href = lSite;
                    } else {

                        if (p_fClosure)
                            p_fClosure(responseText);
                    }
                },
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    console.log("Server Call failed. Retrying...1");
                    OrDzCallBackManager.createClosure(l_objThis, l_objThis.postRequest, [p_strType, p_strUrl, p_strParam, p_fClosure, p_nCBStage + 1])();
                }
            });
            break;
        case 1:
            $['ajax']({
                type: p_strType,
                url: p_strUrl,
                data: p_strParam,
                "beforeSend": function (xhr) { if (lCsrfToken != "") { xhr.setRequestHeader('csrf-token', lCsrfToken); } },
                "success": function (responseText) {

                    if (responseText && ((typeof responseText) == "string") && responseText.indexOf('{"serverRedirect') !== -1) {
                    //    console.log(responseText);
                        var lObjJSON = JSON.parse(responseText);
                        var lRedirectUrl = lObjJSON['serverRedirect'];
                        var lSite = window.location.protocol + "//" + window.location.hostname + lRedirectUrl;
                        window.location.href = lSite;
                    } else {

                        if (p_fClosure)
                            p_fClosure(responseText);
                    }
                },
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    console.log("Server Call failed. Retrying...2");
                    OrDzCallBackManager.createClosure(l_objThis, l_objThis.postRequest, [p_strType, p_strUrl, p_strParam, p_fClosure, p_nCBStage + 1])();
                }
            });
            break;
        case 2:
            $['ajax']({
                type: p_strType,
                url: p_strUrl,
                data: p_strParam,
                "beforeSend": function (xhr) { if (lCsrfToken != "") { xhr.setRequestHeader('csrf-token', lCsrfToken); } },
                "success": function (responseText) {

                    if (responseText && ((typeof responseText) == "string") && responseText.indexOf('{"serverRedirect') !== -1) {
                        //console.log(responseText);
                        var lObjJSON = JSON.parse(responseText);
                        var lRedirectUrl = lObjJSON['serverRedirect'];
                        var lSite = window.location.protocol + "//" + window.location.hostname + lRedirectUrl;
                        window.location.href = lSite;
                    } else {

                        if (p_fClosure)
                            p_fClosure(responseText);
                    }
                },
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    console.log("Server Call failed. Forwarding Response...");
                    if (p_fClosure)
                        p_fClosure(XMLHttpRequest.responseText);
                }
            });
            break;

    }

}

OrDzHttpInterface.prototype.sendRequest = function (p_strUrl, p_strParam, p_fClosure, p_strMethod) {
    var httpRequest = this.getXMLHttpRequest();
    var httpMethod = p_strMethod ? p_strMethod : 'GET';
    if (httpMethod != 'GET' && httpMethod != 'POST') {
        httpMethod = 'GET';
    }
    var httpParams = (p_strParam == null || p_strParam == '') ? null : p_strParam;
    var httpUrl = p_strUrl;
    if (httpMethod == 'GET' && httpParams != null) {
        httpUrl = httpUrl + "?" + httpParams;
    }

    if (httpMethod == 'POST') {
        this.postRequest(httpMethod, httpUrl, p_strParam, p_fClosure, 0);
        return;
    } else if (httpMethod == 'GET') {
        this.postRequest(httpMethod, httpUrl, null, p_fClosure, 0);
    }

    //httpRequest.open(httpMethod, httpUrl, true);
    //httpRequest.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
    //httpRequest.onreadystatechange = OrDzCallBackManager.createClosure(this, this.onReadyStateChange, [p_fClosure], false);
    //httpRequest.send(httpMethod == 'POST' ? httpParams : null);
};

OrDzHttpInterface.prototype.onReadyStateChange = function (p_fClosure) {

    var l_objXHR = this.getXMLHttpRequest();

    if (l_objXHR && l_objXHR.readyState == 4) {
		
		if(l_objXHR.responseText && l_objXHR.responseText.indexOf('{serverRedirect') !== -1) {
			//var wd = window || document;
			//wd.location.reload(true);
			window.location.href = window.location.href;
		
		} else if (p_fClosure) {
            p_fClosure(l_objXHR.responseText);
        }
    }
};

OrDzHttpInterface.prototype.nativeCall = function (p_fMethod, p_strParams, p_fClosure) {

    var l_byteArray = [];

    // Converting params to byte array
    for (var i = 0; i < p_strParams.length; i++) {
        l_byteArray.push(p_strParams.charCodeAt(i))
    }
	
	if("undefined" != typeof dojox) {//in IE9 btoa is not supported		
		p_strParams = dojox['encoding']['base64']['encode'](l_byteArray);
	} else {
		p_strParams = btoa(l_byteArray);
	}

    var httpParams = "methodLabel=" + encodeURIComponent(p_fMethod) + "&methodParams="
            + encodeURIComponent(p_strParams);
    this.sendRequest("default.aspx", httpParams, p_fClosure, "POST");
};

// the file should be with respect to virtual directories. eg: /CDSROOT/tools/capture/samples/ABC.DSN
OrDzHttpInterface.prototype.downloadFile = function (p_strFileName) {
    // "cdnordownloadFile" is used in Default.aspx.cs at Request["methodLabel"]; to set the Request header, Content-Disposition for download
    var httpParams = "methodLabel=" + encodeURIComponent("cdnordownloadFile") + "&methodParams=" + encodeURIComponent(p_strFileName);
    var l_param = "default.aspx?" + httpParams;
    var lForm = document.getElementById("orDownloadFormId");
    if (lForm === null || lForm === undefined) {
        // iframe is created, since form submit is calling $(window)['on']('beforeunload' in default.aspx which is unloaded the session. So target the form to this frame
        var l_objIFrame = document.createElement("iframe");
        l_objIFrame.setAttribute("name", "downloadTargetIFrame");
        l_objIFrame.setAttribute("style", "display:none;");
        document.body.appendChild(l_objIFrame);

        lForm = document.createElement("form");
        lForm.setAttribute("method", "post");
        lForm.setAttribute("id", "orDownloadFormId");
        lForm.setAttribute("target", "downloadTargetIFrame");

        var lHiddenInputField = document.createElement("input");
        lHiddenInputField.setAttribute("type", "hidden");
        lHiddenInputField.setAttribute("name", "ordownloadinputname");
        lHiddenInputField.setAttribute("value", "ordownloadinputname");
        lForm.appendChild(lHiddenInputField);

        var lCsrfToken = "";
        var lCsrfField = document['getElementById']('csrftoken');
        if (lCsrfField != null || lCsrfField != undefined) {
            lCsrfToken = lCsrfField['value'];
        }

        if (lCsrfToken != "") {
            var i = document['createElement']("input");
            i['setAttribute']('name', '_csrf');
            i['setAttribute']('value', lCsrfToken);
            lForm['appendChild'](i);
        }

        document.body.appendChild(lForm);
    }
    lForm.setAttribute("action", l_param);

    lForm.submit();
    /* Not working
    var l_Formdata = new FormData(lForm);
    var http = this.getXMLHttpRequest();
    http.open("POST", l_param, true);
    //http.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
    http.onreadystatechange = function () { alert("Success"); }
    http.send(l_Formdata);
    */
};

OrDzHttpInterface.prototype.signout = function ( p_bClickedLogOutButton, p_fnClosure ) {
 
    var f = document['createElement']("form");
    f['setAttribute']('method', "post");

    f['onerror'] = p_fnClosure;
    f['onreadystatechange'] = p_fnClosure;

    if (p_bClickedLogOutButton == true) {
        f['setAttribute']('action', "/signout?sessionexpired=false");
    } else {
        f['setAttribute']('action', "/signout?sessionexpired=true");
    }

    var lCsrfToken = "";
    var lCsrfField = document['getElementById']('csrftoken');
    if (lCsrfField != null || lCsrfField != undefined) {
        lCsrfToken = lCsrfField['value'];
    }

    if (lCsrfToken != "") {
        var i = document['createElement']("input");
        i['setAttribute']('name', '_csrf');
        i['setAttribute']('value', lCsrfToken);
        f['appendChild'](i);
    }

    document['body']['appendChild'](f);
    f['submit']();
    
};

// the p_objFiles are files returned from element <input type="file" 
OrDzHttpInterface.prototype.uploadFiles = function (p_objFiles, p_fClosure) {

    var l_Formdata = new FormData();
    l_Formdata.append('oruploadinputname', $('#oruploadinputname').get(0).files[0])

    var httpParams = "methodLabel=" + encodeURIComponent("cdnorUploadFile");
    var l_param = "default.aspx?" + httpParams;

    var lCsrfToken = "";
    var lCsrfField = document['getElementById']('csrftoken');
    if (lCsrfField != null || lCsrfField != undefined) {
        lCsrfToken = lCsrfField['value'];
    }
    var l_objXHR = this.getXMLHttpRequest();
    var url = l_param;
    l_objXHR.open('POST', url, true);
    if (lCsrfToken != "") {
        l_objXHR.setRequestHeader('csrf-token', lCsrfToken);
    }
    l_objXHR.onreadystatechange = OrDzCallBackManager.createClosure(this, this.onReadyStateChange, [p_fClosure], false);
    l_objXHR.send(l_Formdata);  // multipart/form-data*/
 
};

OrDzHttpInterface.prototype.nativeSendRequest = function (p_fMethod, p_strParams, p_fClosure, p_strURL, p_strURLParams, p_strURLMethod) {
    this.sendRequest(p_strURL, p_strURLParams, p_fClosure, p_strURLMethod);
};
 
